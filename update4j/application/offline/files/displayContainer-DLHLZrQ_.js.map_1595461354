isplayCommandType.PLAY_VIDEO,\n    DisplayCommandType.SHOW_KEY,\n    DisplayCommandType.SHOW_ONLINE_RESULTS,\n    DisplayCommandType.SHOW_RANKINGS,\n    DisplayCommandType.SHOW_INSPECTION_STATUS,\n    DisplayCommandType.SHOW_ADVANCEMENT\n];\n\nconst AUDIENCE_BAR_UPDATES = [\n    DisplayCommandType.SHOW_PREVIEW,\n    DisplayCommandType.SHOW_MATCH,\n    DisplayCommandType.START_MATCH,\n    DisplayCommandType.ABORT_MATCH,\n    DisplayCommandType.SHOW_RESULTS,\n    DisplayCommandType.SHOW_BLANK,\n    DisplayCommandType.SHOW_VIDEO,\n    DisplayCommandType.REDIRECT,\n    DisplayCommandType.SHOW_AWARD,\n    DisplayCommandType.SHOW_SELECTION,\n]\n\nconst FIELD_UPDATES = [\n    DisplayCommandType.SHOW_SETUP,\n    DisplayCommandType.SHOW_PREVIEW, // allows the FDs to go backwards if the AD goes backwards\n    DisplayCommandType.RANDOMIZE,\n    DisplayCommandType.SHOW_RANDOM,\n    DisplayCommandType.SHOW_MATCH,\n    DisplayCommandType.START_MATCH,\n    DisplayCommandType.ABORT_MATCH,\n    DisplayCommandType.SHOW_RESULTS,\n    DisplayCommandType.REDIRECT,\n    DisplayCommandType.SHOW_TIMEOUT,\n    DisplayCommandType.END_TIMEOUT,\n    // DisplayCommandType.TIMEOUT_UPDATE,\n    DisplayCommandType.SHOW_SELECTION,\n    DisplayCommandType.SHOW_BLANK,\n    DisplayCommandType.SHOW_KEY\n];\n\nconst PIT_UPDATES = [\n    DisplayCommandType.REDIRECT\n]\n\ntype SavedUpdates = Record<`${DisplayCommandType}-${number}`, DisplayCommand<unknown>>\n\ntype PageParams<T> = {\n    latestTimeout?: DisplayCommand<TimeoutUpdate>,\n    command: DisplayCommand<T>,\n    scoreUpdate?: DisplayCommand<ScoreUpdate>,\n    showLeagueRankings?: boolean,\n    nowCallback: (() => number) | (() => any),\n    config: DisplayConfig\n}\n\nfunction narrowPageParams<T>(filter: (msg: DisplayCommand<any>) => msg is DisplayCommand<T>, msg: PageParams<any>): msg is PageParams<T> {\n    return filter(msg.command)\n}\n\nfunction updateReducer(state : SavedUpdates, update: DisplayCommand<unknown>) {\n    if(isCancelMatch(update)) {\n        // All we are doing is filtering, so even though JS collapses the type to a string, it's safe to cast it back\n        return Object.fromEntries(Object.entries(state).filter(([_key, command]) => {\n            if(!isMatchUpdate(command)) {\n                return true\n            }\n\n            return command.params.number == update.params.number && command.index < update.index\n        })) as SavedUpdates\n    }\n\n    return Object.assign({}, state, { [`${update.type}-${update.field}`] : update });\n}\n\nfunction eventFilter(whitelist : DisplayCommandType[], field : string, fieldCount : number) {\n    return (evt : DisplayCommand<unknown>) => {\n        if (field !== 'all' && ((evt.field > 0 && evt.field != parseInt(field)) || (isMatchUpdate(evt) && evt.params.field > 0 && evt.params.field !== parseInt(field)))) {\n            return false;\n        }\n        return whitelist.includes(evt.type);\n    }\n}\n\nconst blankUpdate = { type: DisplayCommandType.SHOW_BLANK, params: {}, ts: 0, index: -1, field: 0, init: false };\nfunction getLastRelevantUpdate(updates : SavedUpdates, config : DisplayConfig, now : number, fieldCount : number, inElims : boolean, inDivisionalElims : boolean) : DisplayCommand<unknown> {\n    const allUpdates = Object.values(updates);\n\n    allUpdates.sort((a, b) => b.ts - a.ts);\n    let filter = eventFilter(AUDIENCE_UPDATES, config.bindToField, fieldCount);\n    let defaultType = DisplayCommandType.SHOW_BLANK;\n    if (config.type === 'field') {\n        filter = eventFilter(FIELD_UPDATES, config.bindToField, fieldCount);\n    }\n    if (config.type === 'cmpBar') {\n        filter = eventFilter(AUDIENCE_BAR_UPDATES, config.bindToField, fieldCount);\n    }\n    if (config.type === 'sponsor') {\n        filter = () => false;\n        defaultType = DisplayCommandType.SHOW_SPONSORS;\n    }\n    if (config.type === 'pit') {\n        filter = eventFilter(PIT_UPDATES, 'all', fieldCount);\n        if(inElims) {\n            defaultType = DisplayCommandType.SHOW_BRACKET;\n        } else if(inDivisionalElims) {\n            defaultType = DisplayCommandType.SHOW_ROTATING_BRACKET;\n        } else {\n            defaultType = DisplayCommandType.SHOW_RANKINGS;\n        }\n    }\n    const filteredUpdates = allUpdates.filter(filter);\n    let bestUpdate = filteredUpdates.length > 0 ? filteredUpdates[0] : { type: defaultType, params: { }, ts: 0, index: -1, field: 0, init: false };\n\n    const startMatch = filteredUpdates.find((up) => up.type === DisplayCommandType.START_MATCH)\n    const abortMatch = filteredUpdates.find((up) => up.type === DisplayCommandType.ABORT_MATCH)\n    // If we are in-match, the best update is always the corresponding START_MATCH\n    if(startMatch !== undefined) {\n        if(bestUpdate.ts - startMatch.ts < (120 + 30 + 8) * 1000 && (abortMatch === undefined || abortMatch.ts < startMatch.ts)) {\n            bestUpdate = startMatch\n        }\n    }\n\n    if(config.type == 'field' && bestUpdate.type == DisplayCommandType.SHOW_RESULTS) {\n        // Look for setups sent *after* the last match started (and that would therefore have otherwise been ignored), otherwise show blank\n        bestUpdate = filteredUpdates.find((up) => startMatch && up.type == DisplayCommandType.SHOW_SETUP && up.ts > startMatch.ts) || blankUpdate;\n    }\n    // When matches are generated field displays should go blank\n    if(config.type == 'field' && isShowSelection(bestUpdate) && bestUpdate.params.state?.immutable) {\n        bestUpdate = blankUpdate;\n    }\n    return bestUpdate;\n}\n\n// nextMatch is the next match _this_ display will see\n// Also need to know if nextMatch != nextMatches[0] - ie we are waiting on another field\nfunction isApplicableTimeout(nextMatch: MatchInfo<MatchTeam>, timeout: TimeoutReference) {\n    switch (timeout.type) {\n        case TimeoutType.G301: return false;\n        case TimeoutType.RUNNING_MATCH: return true; // or do we need this so we can show \"Don't Care\"\n        case TimeoutType.SCHEDULED_START: return (timeout as ScheduledStartTimeout).match === nextMatch?.number;\n        case TimeoutType.T206: return (timeout as T206Timeout).targetMatch === nextMatch.number;\n        case TimeoutType.LAST_ON_FIELD: return (timeout as LastOnFieldTimeout).field === nextMatch.field;\n        case TimeoutType.ALLIANCE_LAST: return [nextMatch.red.seed, nextMatch.blue.seed].includes((timeout as AllianceLastMatchTimeout).alliance);\n        case TimeoutType.T206_MATCH_DEPENDENCY: return true;\n    }\n}\n\nfunction getLastRelevantTimeout(updates : SavedUpdates, config : DisplayConfig, fieldCount : number) {\n    const allUpdates = Object.values(updates);\n    allUpdates.sort((a, b) => b.ts - a.ts);\n    const filteredUpdates = allUpdates.filter(isTimeoutUpdate);\n    if (filteredUpdates.length === 0) {\n        return undefined;\n    }\n    //regardless of field, last update will have the full timeout info - if we are bound to a field, filter the params for TOs we care about downstream\n    return filteredUpdates[0]\n}\n\nfunction getLastRelevantScoreUpdate(updates : SavedUpdates, lastUpdate : DisplayCommand<unknown>) {\n    if (!lastUpdate || !LIVE_UPDATES.includes(lastUpdate.type))\n    {\n        return undefined;\n    }\n\n    const allUpdates = Object.values(updates);\n\n    allUpdates.sort((a, b) => b.ts - a.ts);\n    const filteredUpdates = allUpdates.filter(isScoreUpdate).filter((u) => isMatchUpdate(lastUpdate) && u.params.matchName === lastUpdate.params.matchName);\n    return filteredUpdates.length > 0 ? filteredUpdates[0] : undefined\n}\n\n\nexport const initialConfig : DisplayConfig = {\n    type: 'audience',\n    bindToField: 'all',\n    scoringBarLocation: 'bottom',\n    allianceOrientation: 'standard',\n    mute: false,\n    muteRandomizationResults: false,\n    fieldStyleTimer: true,\n    overlay: false,\n    overlayColor: window.obsstudio ? 'transparent' : '#00FF00',\n    allianceSelectionStyle: 'classic',\n    awardsStyle: 'overlay',\n    dualDivisionRankingStyle: 'sideBySide',\n    rankingsFontSize: 'larger',\n    showMeetRankings: false,\n    rankingsAllTeams: true\n}\n\nfunction transitionKey(type: DisplayCommandType) {\n    if([DisplayCommandType.SHOW_MATCH, DisplayCommandType.START_MATCH, DisplayCommandType.ABORT_MATCH].includes(type)) {\n        return DisplayCommandType.SHOW_MATCH\n    }\n    if([DisplayCommandType.SHOW_SETUP, DisplayCommandType.SHOW_PREVIEW].includes(type)) {\n        return DisplayCommandType.SHOW_PREVIEW\n    }\n    if([DisplayCommandType.SHOW_RANDOM, DisplayCommandType.RANDOMIZE].includes(type)) {\n        return DisplayCommandType.SHOW_RANDOM\n    }\n    return type;\n}\n\nfunction useCursorMovedRecently() {\n    const [movedRecently, setMovedRecently] = useState(true);\n    const mouseMoveTimeout = useRef<number | undefined>(undefined);\n\n    useEffect(() => {\n        mouseMoveTimeout.current = window.setTimeout(() => { setMovedRecently(false)}, 1000)\n        const mouseMoveListener = () => {\n            setMovedRecently(true)\n\n            clearTimeout(mouseMoveTimeout.current);\n            mouseMoveTimeout.current = window.setTimeout(() => { setMovedRecently(false)}, 1000)\n        }\n\n        document.addEventListener('mousemove', mouseMoveListener)\n\n        return () => {\n            if(mouseMoveListener) {\n                document.removeEventListener('mousemove', mouseMoveListener)\n            }\n        }\n    }, [])\n\n    return movedRecently\n}\n\nexport default function DisplayContainer({ reconfigure }:  {reconfigure: (newParams: DisplayParamsType) => void}) {\n    const displayParams = useContext(DisplayParams)\n    const {\n        liveUpdateUrl,\n        eventName,\n        eventCode,\n        fieldCount,\n        isDualDivisionParent,\n        divisionNumber,\n        isLeagueMeet,\n        showMeetRankings,\n        style,\n        missingLeague,\n        isFirstChampionship\n    } = displayParams;\n    const [updates, dispatchUpdate] = useReducer(updateReducer, {} as SavedUpdates);\n    const [rankingData, updateRankingData] = useRankingState();\n    const [config, setConfig] = useState<DisplayConfig>(() => {\n        const searchParams = new URLSearchParams(window.location.search);\n        const valueFromQuery = Object.assign({}, initialConfig, { type: style === 'cloudPit' ? 'pit' : 'audience'});\n        searchParams.forEach((value: string | boolean | number[], key : string) => {\n            if(value === 'true') {\n                value = true;\n            }\n            if(value === 'false') {\n                value = false;\n            }\n            if(key == 'divisionOrder' && typeof value == 'string') {\n                value = value.split(',').map((x) => parseInt(x, 10));\n            }\n            // Query params inherently can't be usefully typed; just assume they came in correctly\n            // @ts-ignore\n            valueFromQuery[key] = value;\n        });\n        return valueFromQuery;\n    });\n    const [dimensions, setDimensions] = useState({\n        height: window.innerHeight,\n        width: window.innerWidth\n    })\n    const [showMeetRankingsVal, setShowMeetRankings] = useState(showMeetRankings);\n    const { isConnected, send, now: nowCallback } = usePersistentWebsocketWithTime(liveUpdateUrl, (data : DisplayCommand<unknown> | {\n        type: 'SHOW_MEET_RANKINGS',\n        value: boolean\n    }) => {\n        if (data.type === 'SHOW_MEET_RANKINGS') {\n            setShowMeetRankings(data.value);\n            return;\n        }\n        if (data.type === DisplayCommandType.SET_CONFIG) {\n            console.log(\"Remote config command:\", data);\n            updateConfig((data as DisplayCommand<SetConfig>).params.config);\n            return;\n        }\n        if(isPitUpdate(data)) {\n            updateRankingData(data.params.message as PitMessage<unknown>);\n            return;\n        }\n        if(isRedirect(data)) {\n            triggerRedirect(data.params.displayParams as DisplayParamsType, data.params.field)\n        }\n        dispatchUpdate(data);\n    }, (s) => s.send(JSON.stringify(config)));\n\n    function updateConfig(newConfig : DisplayConfig) {\n        setConfig(newConfig);\n        window.history.replaceState(displayParams, '', window.location.pathname + '?' + PermissiveURLSearchParams({...newConfig}).toString())\n        send(JSON.stringify(newConfig));\n    }\n\n    useEffect(() => {\n        function handleResize() {\n            setDimensions({\n                height: window.innerHeight,\n                width: window.innerWidth\n            })\n        }\n\n        window.addEventListener('resize', handleResize)\n\n        return () => {\n            window.removeEventListener('resize', handleResize)\n        }\n    }, [])\n\n    const cursorMovedRecently = useCursorMovedRecently();\n\n    const triggerRedirect = useCallback((newParams: DisplayParamsType, field: number) => {\n        const bindingField = (field || 'all').toString();\n        console.log(`Redirecting to display for event: ${newParams.eventCode} (field ${field})`)\n        const targetConfig = Object.assign<{}, DisplayConfig, Partial<DisplayConfig>>({}, config, {bindToField: bindingField})\n        setConfig(targetConfig)\n        window.history.pushState({}, '', window.location.pathname.replace(new RegExp(`/${eventCode}/`), `/${newParams.eventCode}/`) + '?' + PermissiveURLSearchParams({...targetConfig}).toString())\n        send(JSON.stringify(targetConfig));\n        reconfigure(newParams);\n    }, [eventCode, config, displayParams])\n    // For development\n    // @ts-ignore\n    window['triggerRedirect'] = triggerRedirect\n\n    const inElims = rankingData[divisionNumber || 0]?.inElims\n    const inDivisionalElims = isDualDivisionParent && Object.entries(rankingData).filter(([num, _]) => num != '0').every(([_, data]) => data.inElims) || false\n    const lastUpdate = getLastRelevantUpdate(updates, config, nowCallback(), fieldCount ?? 1, inElims, inDivisionalElims);\n    // Disable showing timeouts in FCMP parent, as the show guaranees they are irrelevant\n    const lastTimeout = getLastRelevantTimeout(isFirstChampionship && isDualDivisionParent ? {} : updates, config, fieldCount ?? 1);\n    const lastRelevantScore = getLastRelevantScoreUpdate(updates, lastUpdate);\n\n    // filter assumedNext to the matches relevant to this field (or any if field==0) and pull the first one\n    const nextMatch = lastTimeout?.params.assumedNext.find(m => {\n        if (config.bindToField === 'all') return true;\n        const field = parseInt(config.bindToField);\n        return m.field === field || m.field === 0;\n    });\n    const nextMatchIndex = lastTimeout?.params.assumedNext.indexOf(nextMatch);\n    const expected = nextMatch && lastTimeout?.params.timeouts ? getExpectedStartTimer(nextMatch, lastTimeout?.params.timeouts, isApplicableTimeout, nowCallback) : lastTimeout?.params.timeouts.find(isT206DependencyTimeout);\n\n    const timeoutTimer = useTimer({\n        startAt: expected?.timeoutStart,\n        duration:  (expected?.timeoutDuration ?? 0) / 1000,\n        nowCallback,\n        // TODO unmute on receipt of command - for the next timer only!\n        sounds: true ? {} : {\n            [60]: 'endgameWarning',\n            [0]: 'matchEnd'\n        }\n    });\n\n    const pageParams: PageParams<unknown> = {\n        scoreUpdate: lastRelevantScore,\n        command: lastUpdate,\n        config,\n        nowCallback,\n        latestTimeout: lastTimeout,\n        showLeagueRankings: isLeagueMeet && showMeetRankingsVal && !missingLeague,\n    }\n\n    let page;\n\n    if(narrowPageParams(isShowBlank, pageParams)) {\n        page = <Blank {...pageParams} />\n    } else if(narrowPageParams(isShowVideo, pageParams)) {\n        page = <Video {...pageParams} />\n    } else if(narrowPageParams(isShowKey, pageParams)) {\n        page = <AudienceKey {...pageParams} />\n    } else if(narrowPageParams(isShowSetup, pageParams) || narrowPageParams(isShowPreview, pageParams)) {\n        page = <Preview {...pageParams} />\n    } else if(narrowPageParams(isShowRankings, pageParams)) {\n        page = <Rankings {...pageParams} />\n    } else if(narrowPageParams(isShowInspectionStatus, pageParams)) {\n        page = <InspectionStatus />\n    } else if(narrowPageParams(isShowSponsors, pageParams)) {\n        page = <Sponsors />\n    } else if(narrowPageParams(isRandomization, pageParams)) {\n        page = <Randomization {...pageParams} />\n    } else if(narrowPageParams(isInMatchUpdate, pageParams)) {\n        page = <Match {...pageParams} />\n    } else if(narrowPageParams(isShowResults, pageParams)) {\n        page = <Results {...pageParams} />\n    } else if(narrowPageParams(isShowMessage, pageParams)) {\n        page = <Message {...pageParams} />\n    } else if(narrowPageParams(isShowWifi, pageParams)) {\n        page = <WifiReminder />\n    } else if(narrowPageParams(isShowSafetySecurity, pageParams)) {\n        page = <SafetySecurity />\n    } else if(narrowPageParams(isShowSlideshow, pageParams)) {\n        page = <Slideshow {...pageParams} />\n    } else if(narrowPageParams(isShowSelection, pageParams)) {\n        page = <AllianceSelection {...pageParams} />\n    } else if(narrowPageParams(isShowBracket, pageParams)) {\n        page = <Bracket config={config}/>\n    } else if(narrowPageParams(isShowRotatingBracket, pageParams)) {\n        page = <RotatingBracket config={config}/>\n    } else if(narrowPageParams(isShowOnlineResults, pageParams)) {\n        page = <OnlineResults {...pageParams} />\n    } else if(narrowPageParams(isTimeoutUpdate, pageParams)) {\n        // TODO restore the Timeout page for explicit show command from Control\n        page = <Blank {...pageParams} />\n    } else if(narrowPageParams(isShowAward, pageParams)) {\n        page = <Award {...pageParams} />\n    } else if(narrowPageParams(isShowAdvancement, pageParams)) {\n        page = <Advancement {...pageParams} />\n    } else if(narrowPageParams(isPlayVideo, pageParams)) {\n        page = <PlayVideo {...pageParams} />\n    } else if(narrowPageParams(isRedirect, pageParams)) {\n        page = <Blank {...pageParams} />\n    } else {\n        page = <Debug {...pageParams} />\n    }\n\n    const xZoom = dimensions.width / (config.type == 'cmpBar' ? 1456 : 1920);\n    const yZoom = dimensions.height / (config.type == 'cmpBar' ? 104 : 1080);\n    const displayScale = Math.min(xZoom, yZoom);\n    // as DisplayCommand<any> is gross, but I can't think of a typesafe way to claim a page is *actually* getting the\n    // correct parameters\n    return <div className={`displayRoot ${config.overlay && config.overlayColor === 'transparent' ? 'transparent' : ''}${config.type == 'cmpBar' ? 'cmpBar' : ''} ${cursorMovedRecently ? 'show-cursor' : ''}`} style={{ \"--display-scale\": displayScale }}>\n        {displayParams.avatarUrl ? <link rel=\"stylesheet\" href={displayParams.avatarUrl} /> : null}\n        <Config config={config} setConfig={updateConfig} fieldCount={fieldCount || 1} isConfigured={!window.location.search}\n                isDualDivisionParent={isDualDivisionParent} style={style} showMeetRankings={isLeagueMeet && showMeetRankingsVal} eventName={eventName}\n                cursorMovedRecently={cursorMovedRecently} divisions={displayParams.divisions} />\n        <div className=\"displayContainer\">\n            <DisplayErrorBoundary>\n                <Timeout.Provider value={{timer: timeoutTimer, nextMatch: nextMatch, nextMatchIndex: nextMatchIndex, timeout: expected}}>\n                <RankingData.Provider value={rankingData}>\n                    <TransitionGroup component={null}>\n                        {/* Play video must not  transition or the audio will continue to play while it is invisible */}\n                        <CSSTransition key={transitionKey(lastUpdate.type)} timeout={lastUpdate.init || lastUpdate.type == 'PLAY_VIDEO' ? 0 : 5000} classNames=\"page\">\n                            {page}\n                        </CSSTransition>\n                    </TransitionGroup>\n                </RankingData.Provider>\n                </Timeout.Provider>\n            </DisplayErrorBoundary>\n            <EnsureAudio config={config} />\n            {isConnected ? null : <DisconnectedBanner />}\n        </div>\n    </div>\n}"],"names":["Config","config","setConfig","isConfigured","fieldCount","isDualDivisionParent","style","showMeetRankings","eventName","cursorMovedRecently","divisions","showConfig","setShowConfig","useState","useEffect","clickListener","event","simpleDivisions","key","value","jsxs","Fragment","jsx","t","ConfigDialog","DisplayParams","createContext","MiniTimeoutType","MiniTimeoutType2","getTimeoutRender","timeout","standardText","isT206Timeout","isT206DependencyTimeout","isAllianceLastTimeout","isRunningMatchTimeout","renderTime","MiniTimeout","type","displayParams","useContext","renderData","EventLogo","logo","ReactSVG","Blank","assetPath","backgroundPath","useAsset","Timeout","Debug","command","scoreUpdate","params","Video","alternatePresentingSponsor","getContext","fragment","canvas","getTextWidth","currentText","font","context","metrics","useTextWidth","options","textOptions","useMemo","refOptions","_a","_b","computedStyles","ScrollingText","children","className","width","targetRef","useRef","scrollSpeed","setScrollSpeed","calculateScrollingText","useCallback","node","contentWidth","resizeObserver","fillerSpanStyle","EventName","invertLogo","sponsorAttr","formattedEventName","escape","OneThird","matchName","vflip","themeLogo","gameLogo","name","truncateMatchNameForCmpBar","ret","CmpBar","pageTitle","timer","hideMiddleBar","eventNameOverride","shortEventName","sponsorSuffix","cmpLogo","nameWrapper","sponsorIntro","isRankingMessage","msg","PitMessageType","isResultMessage","isOnlineURLMessage","isBracketMessage","placeholderRankingData","reducer","state","divData","res","results","m","r