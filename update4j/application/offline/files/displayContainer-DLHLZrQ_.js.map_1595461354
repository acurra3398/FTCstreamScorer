        // Never show - we are done\n                return undefined;\n            }\n        }\n        if (isRunningMatchTimeout(timeout.timeout)) {\n            return {time: renderTime(timeout.timer.remaining), text: standardText};\n        }\n        // If not the next match, show the max of the calculated time and the not-yet-applied TBDs\n        if ((timeout.nextMatchIndex ?? 0) > 1) {\n            if (timeout.timer.remaining < 5 * 60 + 16) {\n                return {time: \"5:22+\", text: standardText};\n            }\n        }\n        if ((timeout.nextMatchIndex ?? 0) > 0) {\n            if (timeout.timer.remaining < 2 * 60 + 38) {\n                return {time: \"2:41+\", text: standardText};\n            }\n        }\n        if (timeout.timer.remaining >= 60 * 100) {\n            // handle long lunch or overnight breaks\n            return {time: \"99:59\", text: t`Next match starts after break`};\n        }\n        return {time: renderTime(timeout.timer.remaining), text: standardText};\n    }\n    return undefined;\n}\n\nexport default function MiniTimeout({type, config, timeout} : {\n    type: MiniTimeoutType,\n    config: DisplayConfig,\n    timeout: ActiveTimeout\n}) {\n    const displayParams = useContext(DisplayParams);\n\n    const renderData = getTimeoutRender(timeout);\n    console.log(renderData)\n\n    if (!renderData) {\n        return null;\n    }\n\n    if (type === MiniTimeoutType.EMBED) {\n        return <div className={\"miniTimeout embed\"}>\n            <div className={\"timer\"}>\n                {renderData.time}\n            </div>\n        </div>\n    }\n\n    if (type === MiniTimeoutType.BANNER || type === MiniTimeoutType.TOP_RIGHT) {\n        return <div className={`miniTimeoutBanner ${config.scoringBarLocation === 'top' ? \"vflip\" : \"\"} ${type === MiniTimeoutType.TOP_RIGHT ? \"topright\" : \"\"}`}>\n            <div className={`miniTimeout banner`}>\n                <div className={\"timer\"}>\n                    {rende