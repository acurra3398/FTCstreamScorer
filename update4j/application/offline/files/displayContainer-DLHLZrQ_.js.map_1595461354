gMatch {\n  number: string,\n  redScore: number,\n  blueScore: number,\n  redTeams: string,\n  blueTeams: string,\n  winner: string\n}\n\ninterface RankingTeam {\n  rank: string | number,\n  number: string,\n  rp: string,\n  tbp1: string,\n  tbp2: string,\n  record: string,\n  matchesPlayed: number,\n  leagueRank: number | string\n}\n\ninterface RankingAlliance {\n  rank: string | number,\n  seed: number,\n  teams: string,\n  rp: string,\n  tbp1: string,\n  tbp2: string,\n  matchesPlayed: number,\n  record: string,\n}\n\nexport interface DivisionRankingData {\n  matchList: RankingMatch[],\n  matchCount: number,\n  matchesPlayed: number,\n  matchesPerTeam: number,\n  lastUpdatedAt: number,\n  rankings: RankingTeam[],\n  allRankings?: RankingTeam[],\n  url: string | null,\n  inElims: boolean,\n  bracket?: GenericBracket,\n  playoffStructure?: PlayoffStructure<SeriesDescription>\n}\n\nconst placeholderRankingData: DivisionRankingData = {\n  matchList: [],\n  matchCount: 0,\n  matchesPlayed: 0,\n  matchesPerTeam: 0,\n  lastUpdatedAt: 0,\n  rankings: [],\n  allRankings: undefined,\n  url: null,\n  inElims: false,\n  bracket: undefined\n}\n\nexport function reducer(state : Record<number, DivisionRankingData>, msg : PitMessage<unknown>) {\n  const divData = Object.assign({}, placeholderRankingData, state[msg.division]);\n  if (isRankingMessage(msg)) {\n    Object.assign<DivisionRankingData, Partial<DivisionRankingData>>(divData, {\n      [msg.type === PitMessageType.ALL_RANKINGS ? 'allRankings' : 'rankings']:\n        msg.payload.map((res) : RankingTeam => ({\n          rank: res.rank > 0 ? res.rank : (res.rank === 0 ? '-' : 'NP'),\n          number: res.team,\n          rp: res.RP,\n          tbp1: res.TBP1,\n          tbp2: res.TBP2,\n          record: `${res.wins}-${res.losses}-${res.ties}`,\n          matchesPlayed: res.plays,\n          leagueRank: res.leagueRank > 0 ? res.leagueRank : (res.leagueRank === 0 ? '-' : 'NP')\n        }))\n    })\n  } else if (isResultMessage(msg)) {\n    const results = msg.payload.filter((m) => m.played).map((m) => ({\n      number: m.match,\n      redScore: m.red,\n      blueScore: m.blue,\n      redTeams: m.redTeams.join(\"\\n\"),\n      blueTeams: m.blueTeams.join(\"\\n\"),\n      winner: m.winner\n    }));\n    Object.assign<DivisionRankingData, Partial<DivisionRankingData>>(divData, {\n      matchList: results,\n      matchCount: msg.payload.filter(m => m.match.startsWith(\"Q\")).length,\n      matchesPlayed: results.filter(r => r.number.startsWith(\"Q\")).length,\n      matchesPerTeam: msg.matchesPerTeam,\n      lastUpdatedAt: msg.lastUpdatedAt\n    })\n  } else if (isOnlineURLMessage(msg)) {\n    Object.assign<DivisionRankingData, Partial<DivisionRankingData>>(divData, {\n      url: msg.payload[0]\n    })\n  } else if (isBracketMessage(msg)) {\n    Object.assign<DivisionRankingData, Partial<DivisionRankingData>>(divData, {\n      bracket: msg.payload[0],\n      playoffStructure: msg.structure,\n      inElims: msg.inElims\n    })\n  }\n  return Object.assign({}, state, { [msg.division]: divData });\n}\n\nexport const RankingData = createContext<Record<number, DivisionRankingData>>({})\nexport const useRankingState = () => useReducer(reducer, {})\n","import \"./MiniBracket.css\";\nimport {\n  SeriesAllianceState\n} from \"../../javaTypes\";\nimport {ReactNode, useContext, useEffect, useMemo, useRef, useState} from \"react\";\nimport { RankingData } from \"../rankingDataState\";\nimport { t } from \"../../i18n\";\nimport {\n  bracketTargets,\n  isBasicAllianceDescription,\n  isDoubleElimsSeriesDescription,\n  isSeriesAllianceDescription\n} from \"../util\";\n\nexport default function MiniBracket({divisionNumber = 0, highlightSeries} : {\n  divisionNumber?: number,\n  highlightSeries?: number\n}) {\n  const bracketContainer = useRef<HTMLDivElement | null>(null);\n  const state = useContext(RankingData);\n\n  const bracket = state[divisionNumber]?.bracket;\n  const structure = state[divisionNumber]?.playoffStructure;\n  const sortedSeries = useMemo(() => {\n    return structure ? [...structure.series].sort((a, b) => {\n      if(a.round != b.round) {\n        return a.round - b.round;\n      }\n      if(isDoubleElimsSeriesDescription(a) && isDoubleElimsSeriesDescription(b) && a.bracketHalf != b.bracketHalf) {\n        return a.bracketHalf == 'LOWER' ? 1 : -1;\n      }\n      // If both series have any winners, sort by the series numbers of those winners\n      const aWinners: number[] = [isSeriesAllianceDescription(a.red) ? a.red.series : null, isSeriesAllianceDescription(a.blue) ? a.blue.series : null].filter(m => m != null) as number[]\n      const bWinners: number[] = [isSeriesAllianceDescription(b.red) ? b.red.series : null, isSeriesAllianceDescription(b.blue) ? b.blue.series : null].filter(m => m != null) as number[]\n      const firstWinner = Math.min(...aWinners, ...bWinners, 999)\n      if(firstWinner < 999) {\n        return aWinners.includes(firstWinner) ? -1 : 1\n      }\n      return a.series - b.series\n    }) : undefined;\n  }, [structure]);\n\n  const maxLayers = Math.max(...(sortedSeries?.filter(p => bracket?.seriesData[p.series - 1]).map(p => p.round) || [0]))\n  const matchesInRoundHalves = sortedSeries?.reduce((acc, p) => {\n    acc[`${p.round}-${isDoubleElimsSeriesDescription(p) ? p.bracketHalf : 'CENTER'}`] ||= 0\n    acc[`${p.round}-${isDoubleElimsSeriesDescription(p) ? p.bracketHalf : 'CENTER'}`]++\n    return acc\n  }, {} as Record<string, number>);\n  const maxInRoundHalf = Math.max(...(matchesInRoundHalves ? Object.values(matchesInRoundHalves) : [1]))\n  let firstMinimalRound = matchesInRoundHalves ? Math.min(...Object.entries(Object.entries(matchesInRoundHalves).reduce((acc, e) => {\n    const round = e[0].split('-')[0]\n    acc[round] ||= []\n    acc[round].push(e[1])\n    return acc\n  }, {} as Record<string, number[]>)).filter((e) => e[1].length <= 2 && (!e[1][0] || e[1][0] == 1) && (!e[1][1] || e[1][1] == 1)).map(e => parseInt(e[0])), 999) : 1\n  // Don't need special formatting for the first round for sufficiently small brackets\n  if (maxInRoundHalf < 3 && maxLayers < 6) {\n    firstMinimalRound = 0;\n  }\n\n\n  const columnGap = 200 / (maxLayers - 1);\n  const [svgPaths, setSvgPaths] = useState<ReactNode>();\n\n  // TODO this should be useLayoutEffect\n  useEffect(() => {\n    if(!bracketContainer.current || !sortedSeries) {\n      return;\n    }\n\n    const scale = parseFloat(getComputedStyle(bracketContainer.current).getPropertyValue(\"--display-scale\"));\n    const baseRect = bracketContainer.current.getBoundingClientRect();\n\n    const paths: ReactNode[] = []\n    for(let i = 0; i < sortedSeries.length; i++) {\n      const pairing = sortedSeries[i];\n      const element = bracketContainer.current.getElementsByClassName(`series-${pairing.series}`)[0];\n      if(!element) {\n        continue;\n      }\n      const labelRect = element.getElementsByClassName(\"allianceGroupLabel\")[0].getBoundingClientRect();\n\n      const labelStart = { x: (labelRect.x + labelRect.width - baseRect.x) / scale, y: (labelRect.y + labelRect.height / 2 - baseRect.y) / scale };\n\n      const winnerMatch = sortedSeries.find(p =>\n          isSeriesAllianceDescription(p.red) && p.red.state == SeriesAllianceState.WINNER && p.red.series == pairing.series ||\n          isSeriesAllianceDescription(p.blue) && p.blue.state == SeriesAllianceState.WINNER && p.blue.series == pairing.series)\n      if(!winnerMatch) {\n        continue;\n      }\n      const winnerContainer = bracketContainer.current.getElementsByClassName(`series-${winnerMatch.series}`)[0];\n      if(!winnerContainer) {\n          continue;\n      }\n      const winnerLabel = winnerContainer.getElementsByClassName(\"allianceGroupLabel\")[0];\n      const winnerPos = isSeriesAllianceDescription(winnerMatch.red) && winnerMatch.red.state == SeriesAllianceState.WINNER && winnerMatch.red.series == pairing.series ? 'top' : 'bottom'\n      const winnerLabelRect = winnerLabel.getBoundingClientRect();\n      const winnerLabelStart = { x: (winnerLabelRect.x - baseRect.x) / scale, y: (winnerLabelRect.y + winnerLabelRect.height * (winnerPos == 'top' ? 1 : 2) / 3 - baseRect.y) / scale };\n\n      paths.push(<path key={pairing.series} d={`M ${labelStart.x} ${labelStart.y} h ${columnGap * (1 / 3)} V ${winnerLabelStart.y} H ${winnerLabelStart.x}`} stroke=\"white\" fill=\"transparent\" strokeWidth=\"4\"/>)\n    }\n    setSvgPaths(paths)\n  }, [sortedSeries, bracketContainer.current]);\n\n  const [highlightWinnerTarget, highlightLoserTarget] = bracketTargets(structure, highlightSeries)\n  const highlightSeriesDescription =  highlightSeries ? structure?.series[highlightSeries - 1] : undefined\n  let highlightInfo: ReactNode = undefined\n  const level = divisionNumber > 0 ? t`division` : t`event`;\n  if(highlightSeriesDescription?.isFinals && highlightWinnerTarget) {\n    // This must be the first finals match, so show appropriate text\n    highlightInfo = <div className={\"mini-bracket-sidebar\"}>\n      <span>\n        <i className={\"fa fa-trophy mr-2\"}></i>\n        <span className={\"redLabel\"}>{t`Red`}</span> {t`wins`}\n        <i className={\"fa fa-arrow-right\"}></i><br/>\n        {t`Red wins ${level}`}\n      </span>\n      <span>\n        <i className={\"fa fa-trophy mr-2\"}></i>\n        <span className={\"blueLabel\"}>{t`Blue`}</span> {t`wins`}\n        <i className={\"fa fa-arrow-right\"}></i><br/>\n        {t`Additional Match (${highlightWinnerTarget.series})`}<br/>\n      </span>\n    </div>\n  } else if(highlightSeriesDescription?.isFinals) {\n\n    // This must be the second finals match, so show appropriate text\n    highlightInfo = <div className={\"mini-bracket-sidebar\"}>\n      <span><i\n          className={\"fa fa-trophy\"}></i> { t`Winner\\nis ${level} winner`}</span>\n      <span><i\n          className={\"fa fa-arrow-down\"}></i> {t`Loser\\nis ${level} finalist`}</span>\n    </div>\n  } else if (hig