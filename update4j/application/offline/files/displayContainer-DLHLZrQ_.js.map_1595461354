    sponsor: Sponsor\n}) {\n    return <>\n        {sponsor.title ? <div className={\"singleSponsorTitle\"}>{sponsor.title}</div> : sponsor.title}\n        {sponsor.logo ? <div className={`sponsorImageContainer ${sponsor.title ? 'withTitle' : ''} ${sponsor.position ? `position${sponsor.position}` : ''}`}>\n        <img className=\"sponsorImage\" src={sponsor.logo} />\n    </div> : null }\n    </>;\n}\n\nfunction MultiSponsor({sponsors, isGlobal} : {\n    sponsors: Sponsor[],\n    isGlobal: boolean\n}) {\n    return <div className={`sponsorContainer ${isGlobal ? 'twoSponsors' : ''}`}>{sponsors.map((sponsor, idx) => {\n        return <div key={sponsor.sponsorId} className={`sponsorCell`}>\n            <div className={\"sponsorTitle\"}>{sponsor.title}</div>\n            {sponsor.logo ? <div className={\"sponsorImageContainerSm\"}>\n                <img className=\"sponsorImage\" src={sponsor.logo} />\n            </div> : <div className={\"sponsorName\"}>{sponsor.name}</div> }\n        </div>\n    })}</div>;\n}\n\nexport function StatelessSponsors({ sponsors}: {\n    sponsors?: Sponsor[]\n}) {\n    let chunkLevel = sponsors ? sponsors[0].level : null;\n\n    return <Primary>\n        {/* Make sure the presenting sponsor gets a standalone page */}\n        {sponsors ? (chunkLevel === 'GLOBAL' && sponsors[0].position === 1 ? <SingleSponsor sponsor={sponsors[0]} /> : <MultiSponsor sponsors={sponsors} isGlobal={chunkLevel === 'GLOBAL'} />) : null}\n\n    </Primary>;\n}\n\nexport default function Sponsors() {\n    const params = useContext(DisplayParams)\n    const chunkedSponsors = useSponsors(params.sponsorsUrl)\n    const currentChunkIdx = useSlides(chunkedSponsors.length)\n    const chunk = chunkedSponsors[currentChunkIdx];\n\n    if(!params.sponsorsUrl) {\n        return <>{\"Cannot display sponsors-missing url\"}</>;\n    }\n\n    return <StatelessSponsors sponsors={chunk} />\n}","import Primary from \"../layouts/Primary\";\n\nimport './Message.css'\nimport {DisplayCo