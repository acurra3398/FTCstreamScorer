ore ? <span className={\"highScoreBanner\"}><i className={\"fa fa-star\"}></i> {highScoreFlavor} <i className={\"fa fa-star\"}></i></span> : null}\n        </div>\n        <div className={`resultsUpperWrapper`}>\n            {isPlayoffs && teams[0].card ? <div className={\"elimsCard\"}>{t`Received Card:`} <RefCard card={teams[0].card} /></div> : null}\n            {items}\n            {isPlayoffs && target ? <div className={\"elimsMovement\"}>{target}</div> : null}\n        </div>\n        <div className={\"resultsComponents\"}>\n            <span>{scoreDetail.autoLeavePoints || 0}</span>\n            <span>{(scoreDetail.autoArtifactPoints || 0) + (scoreDetail.teleopArtifactPoints || 0) + (scoreDetail.teleopDepotPoints || 0)}</span>\n            <span>{(scoreDetail.autoPatternPoints || 0) + (scoreDetail.teleopPatternPoints || 0)}</span>\n            <span>{scoreDetail.teleopBasePoints || 0}</span>\n            <span>{otherScoreDetail.foulPointsCommitted || 0}</span>\n            {isPlayoffs && !target && record != undefined ? <span className={\"win-banner\"}>{record}</span> : null}\n        </div>\n        {hasRank ? <div className={\"earnedRpContainer\"}>\n            <span>{t`Ranking Points`}</span>\n            <div className={\"earnedRp\"}>\n                <span className={`rpBadge ${earnedFirstWinRp ? 'earned' : ''}`}><i className={\"fa fa-trophy\"}></i></span>\n                <span className={`rpBadge ${earnedSecondAndThirdWinRp ? 'earned' : ''}`}><i className={\"fa fa-trophy\"}></i></span>\n                <span className={`rpBadge ${earnedSecondAndThirdWinRp ? 'earned' : ''}`}><i className={\"fa fa-trophy\"}></i></span>\n                <span className={`rpBadge ${earnedFirstGameRp ? 'earned' : ''}`}><i className={\"fa fa-robot\"}></i></span>\n                <span className={`rpBadge ${earnedSecondGameRp ? 'earned' : ''}`}>\n                    <ReactSVG src={goalRPIcon} />\n                </span>\n                <span className={`rpBadge ${earnedThirdGameRp ? 'earned' : ''}`}>\n                    <div className={\"motif\"}>\n                        <ReactSVG src={goalRPIcon} />\n                        <ReactSVG src={goalRPIcon} />\n                        <ReactSVG src={goalRPIcon} />\n                    </div>\n                </span>\n            </div>\n        </div> : null}\n    </div>;\n}\n\nfunction colorToVideo(key?: AllianceColor | null): VideoColors {\n    if(key == AllianceColor.RED) {\n        return 'red'\n    } else if (key == AllianceColor.BLUE) {\n        return 'blue'\n    } else {\n        return 'tie'\n    }\n}\n\nconst bubblePaths = [\n    \"m 0,0 c -0.0352,0.1087 -0.0839,0.1309 -0.1466,0.0659 0.0342,-0.1128 0.083,-0.1349 0.1466,-0.0659 m -1.1604,1.8141 c 0.0204,2.6157 -3.5346,2.6464 -3.379,0.0568 0.1241,-2.0712 3.3619,-2.267 3.379,-0.0568 m -7.1943,1.2329 c 0.0298,5.7931 9.086,6.4712 10.4511,1.1915 1.6697,-6.4576 -10.5503,-8.1413 -10.4511,-1.1915\",\n    \"m 0,0 c -3.2955,-4.3918 6.6458,-6.3322 9.1875,-4.187 3.1918,2.6942 -6.0967,8.3975 -9.1875,4.187 m -2.7185,-10.4028 c -8.4003,6.8975 4.136,17.7172 11.6241,11.999 3.9438,-3.0114 2.9333,-7.876 -0.421,-10.8788 -3.4638,-3.0998 -7.2182,-3.4089 -11.2031,-1.1202\",\n    \"m 0 0 c -2.82 4.2349 -7.5838 -3.8516 -6.494 -6.7346 c 1.3683 -3.6201 9.2187 2.7347 6.494 6.7346 m -8.2496 5.5861 c 8.6194 5.2102 14.2025 -9.0823 6.8951 -13.8593 c -3.8483 -2.516 -7.7868 -0.1318 -9.3743 3.7257 c -1.6393 3.9831 -0.7504 7.3585 2.4792 10.1336\",\n    \"m 0,0 c 1.3038,-1.782 2.3116,-4.4888 0.305,-6.2541 -1.4238,-1.2528 -3.7594,-1.1725 -5.2188,-0.0194 -3.8419,3.0357 1.1495,9.8294 4.9138,6.3764 l 0.0104,0.007 c 0,-0.0388 0,-0.0776 0,-0.1164 l -0.0104,0.007 z\"\n]\n\nconst HIGH_SCORE_TEXT: Record<HighScoreLevel, string> = {\n    [HighScoreLevel.DIVISION]: t`Division High Score`,\n    [HighScoreLevel.EVENT]: t`Event High Score`,\n    [HighScoreLevel.REGION]: t`Region High Score`,\n    [HighScoreLevel.WORLD]: t`World High Score`,\n}\n\nexport default function Results({config, command, nowCallback, showLeagueRankings, missingLeague} : {\n    config: DisplayConfig,\n    command: DisplayCommand<ShowResults>,\n    nowCallback: () => number,\n    showLeagueRankings?: boolean,\n    missingLeague?: boolean\n}) {\n    const [lateRender, setLateRender] = useState(false);\n    const [resultsHidden, setResultsHidden] = useState(false);\n    const params = useContext(DisplayParams);\n    useEffect(() => {\n        {/* Don't play the video if we refresh the page */}\n        setLateRender(nowCallback() - command.ts > 1000);\n        setResultsHidden(false);\n        setTimeout(() => setResultsHidden(true), 12500)\n    }, [command.ts]);\n    const videoDisabled = command.params.singleTeam || lateRender;\n    const { remaining, running } = useTimer({\n        startAt: command.ts,\n        duration: videoDisabled ? 0 : 6,\n        nowCallback: nowCallback\n    });\n    const state = useContext(RankingData);\n\n    const shouldHaveConfetti = command.params.isEventConclusion;\n    const { running: confettiRunning } = useTimer({\n        startAt: command.ts,\n        duration: shouldHaveConfetti ? 65 : 0,\n        nowCallback: nowCallback\n    })\n\n    // TODO update for CS\n    if(config.type == 'cmpBar') {\n        let winnerText = t`Tie!`;\n        if(command.params.winner == AllianceColor.BLUE) {\n            winnerText = t`Blue Wins!`;\n        }\n        if(command.params.winner == AllianceColor.RED) {\n            winnerText = t`Red Wins!`;\n        }\n\n        if(resultsHidden) {\n            return <Blank config={config} />;\n        } else {\n            return <div>\n                <div className={`winner ${command.params.winner?.toLowerCase()}`}>{winnerText}</div>\n                <ReactConfetti\n                    width={1456}\n                    height={120}\n                    initialVelocityY={0.5}\n                    gravity={-0.1}\n                    numberOfPieces={20}\n                    confettiSource={{\n                        x: 0,\n                        y: 120,\n                        w: 1456,\n                        h: 0\n                    }}\n                    // Pretend paths are colors to pass cleanly through library\n                    colors={bubblePaths}\n                    tweenDuration={15000}\n                    drawShape={function(this: Particle, ctx){\n                        ctx.fillStyle = ctx.strokeStyle = '#005aaa';\n                        //reverse internal transformations\n                        ctx.rotate(-this.angle);\n                        ctx.scale(1, 1 / this.rotateY);\n                        ctx.rotate(-this.angle);\n\n                        ctx.fill(new Path2D(this.color));\n                    }}\n                    />\n            </div>;\n        }\n    }\n\n    const structure = state[params.divisionNumber || 0]?.playoffStructure;\n    const seriesDescription = structure?.series.find(s => s.series == command.params.series);\n    const [winnerTarget, loserTarget] = command.params.isEventConclusion || command.params.isDivisionConclusion ? [null, null] : bracketTargets(structure, command.params.series);\n    const winnerTargetColor = winnerTarget ? ((isSeriesAllianceDescription(winnerTarget.red) && winnerTarget.red.series == command.params.series && winnerTarget.red.state == SeriesAllianceState.WINNER) ? t`Red` : t`Blue`) : null\n    const loserTargetColor = loserTarget ? ((isSeriesAllianceDescription(loserTarget.red) && loserTarget.red.series == command.params.series && loserTarget.red.state == SeriesAllianceState.LOSER) ? t`Red` : t`Blue`) : null\n    const isQD = false; // TODO for premier\n    const winnerTargetString = winnerTarget ?\n        <>{t`Advances to:`} <br/> {winnerTarget.name} <br/> Will be {winnerTargetColor} Alliance</> :\n        ((params.divisionNumber ?? 0) > 0 ? (<>{t`Division Winner`}<br/>{t`Advances to:`}<br/>{params.isFirstChampionship ? t`Championship Playoffs` : (isQD ? t`Event Playoffs`: t`Event Finals`)}</