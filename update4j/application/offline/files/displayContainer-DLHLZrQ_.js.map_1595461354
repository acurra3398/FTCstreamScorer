   caption?: boolean,\n    color?: 'red' | 'blue' | 'orange' | 'grey',\n    winCount?: ReactNode,\n    compressed?: boolean,\n    vertical?: boolean,\n    teamsBeforeSeed?: boolean,\n    teamCount?: number,\n    withAvatar?: boolean\n}) {\n    if(caption) {\n        alliance = teamCount == 3 ? [t`Captain`, t`1st Pick`, t`2nd Pick`] : [t`Captain`,  t`Partner`]\n    }\n    if(!alliance) {\n        alliance = ['']\n    }\n\n    const teams = teamCount || 3\n    const allianceElements = alliance.slice(0, teams).map((team, idx) => {\n        if(!team) return <span key={idx} className={\"allianceTeamNumber\"}>&nbsp;</span>;\n        const teamStr = typeof team == \"object\" && \"displayNumber\" in team ? team.displayNumber : team;\n        return <span key={idx} className={`allianceTeamNumber ${withAvatar ? 'withAvatar' : ''}`}>{withAvatar ? <span className={`team-avatar team-${teamStr}`}></span> : null}<span className={\"numberText\"}>{teamStr}</span></span>\n    })\n    const seedElement = seed || caption ? <span className={`seed ${typeof seed == 'string' ? '' : 'big'}`}>{seed}</span> : null;\n    const teamsElement = <div className={\"teamsWrapper\"}>{allianceElements}</div>;\n    const teamAndSeed = teamsBeforeSeed ? [teamsElement, seedElement] : [seedElement, teamsElement];\n    return <div className={`allianceBar alliance${seed} ${caption ? 'caption' : ''} ${color || ''} ${compressed ? 'compressed' : ''}`} style={{'--team-count': teams}}>\n        {vertical ? <div className={\"verticalContainer\"}>{teamAndSeed}</div> : teamAndSeed}\n        {winCount != null? <span className={\"winCount\"}>{winCount}</span> : null}\n    </div>;\n}","import Primary from \"../layouts/Primary\";\n\nimport './AllianceSelection.css'\nimport ScrollingTable from \"../components/ScrollingTable\";\nimport {t} from \"../../i18n\";\nimport AllianceBar from \"../components/AllianceBar\";\nimport {DisplayCommand, ShowSelection, TeamSelectionState} from \"../../javaTypes\";\nimport {DisplayConfig} from \"../Config\";\nimport Blank from \"./Blank\";\n\nfunction AllianceColumn({teams, lastColumn} : {\n    teams: TeamSelectionState[],\n    lastColumn: boolean\n}) {\n    return <ScrollingTable\n                    colorCol={'color'}\n                    classCol={'className'}\n                    height={'815px'}\n                    keyCol={'rawNumber'}\n                    disableScroll={!lastColumn}\n                    columns={[\n                        {\n                            name: t`Rank`,\n                            field: 'rank'\n                        },\n                        {\n                            name: t`Team`,\n                            field: 'number'\n                        }\n                    ]}\n                    noHeading\n                    data={teams.map((t, idx) => {\n                            const unavailable = t.picked || t.captain || t.declined || t.removed;\n                            return {\n                                rawNumber: t.number,\n                                rank: t.rank,\n                                number: t.displayNumber,\n                                className: unavailable ? 'unavailable' : ''\n                            }\n                        })}\n    />\n}\n\nexport default function AllianceSelection({command, config} : {\n    command: DisplayCommand<ShowSelection>,\n    config: DisplayConfig\n}) {\n    if(config.type == 'cmpBar') {\n      return <Blank config={config} />;\n    }\n\n    const hybrid = config.overlay && config.allianceSelectionStyle === 'hybrid'\n    const state = command.params.state\n    const currentCaptain = state?.alliances && state.nextSlot < state.alliances.length * 2 ? (state?.teams || []).filter(t => !t.removed && !t.captain && !t.picked)[0].number : 0\n    const allTeams = state ? state.teams.map(t => {\n        if(t.number == currentCaptain) {\n            return {\n                ...t,\n                captain: true\n            }\n        } else {\n            return t\n        }\n    }) : []\n    const compactList = allTeams.length > (hybrid ? 48 : 60);\n    const hugeList = !hybrid && allTeams.length < 25;\n    const perColumn = compactList ? 16 : (hugeList ? 10 : 12);\n\n    const chunkedTeams = allTeams.reduce<TeamSelectionState[][]>((resultArray, item, index) => {\n        // 12 per chunk, never more than 4 columns\n        const chunkIndex = Math.min(Math.floor(index / perColumn), hybrid ? 4 : 5);\n\n        if (!resultArray[chunkIndex]) {\n            resultArray[chunkIndex] = [] // start a new chunk\n        }\n\n        resultArray[chunkIndex].push(item);\n\n        return resultArray\n    }, [])\n    // never let there be less than 3 columns in non-hybrid\n    for(let i = chunkedTeams.length; i <= (hybrid ? 1 : 2); i++) {\n        chunkedTeams[i] = [];\n    }\n    const currentAlliance = state ? state.nextSlot >= (state.alliances.length * 2) ? (state.alliances.length * 3 - 1) - state.nextSlot : state.nextSlot / 2 : -1;\n    const allianceCount = state?.alliances.length || 4;\n\n    return <Primary className={`selectionPage ${hybrid ? 'hybridSelection' : 'classicSelection'} ${hybrid && config.overlayColor == 'transparent' ? 'transparent' : 'null'}`}>\n            <div className={\"allianceBarsWrapper\"}>\n               <div className={\"allianceBars\"} style={{\n                   gridTemplateRows: `repeat(${hybrid ? Math.ceil(allianceCount / 2) : allianceCount}, minmax(0, 1fr))`,\n               }}>\n                   {hybrid ? null : <AllianceBar caption teamCount={state?.teamsPerAlliance || 2} />}\n                   {state ?\n                       state.alliances.map((alliance, idx) =>\n                           // remove non-finalized alliance captains (treat the next captain as already finalized)\n                           <AllianceBar withAvatar key={idx + 1} seed={idx + 1} teamCount={state.teamsPerAlliance} alliance={alliance.map(t => state.teams\n                               .some(t2 => t == t2.number && !t2.captain && !t2.picked) && state.nextSlot != (idx) * 2 ? undefined : t)\n                               .filter((t): t is number => !!t)\n                               .map(t => (state.teams.find(t2 => t == t2.number) || { displayNumber: t.toString() })) } color={idx == currentAlliance ? \"orange\" : undefined}/>) :\n                       [1,2,3,4].map((i) => <AllianceBar key={i} seed={i} alliance={[]}/>)}\n               </div>\n            </div>\n           <div className=\"chromaBackground\" style={{ background: config.overlayColor }} />\n           <div className={`teamListContainer ${compactList ? 'compact' : ''} ${hugeList ? 'huge' : ''}`}>\n               {chunkedTeams.map((teamList, i) => <AllianceColumn key={i} teams={teamList} lastColumn={i === chunkedTeams.length - 1}/>)}\n           </div>\n    </Primary>;\n}","import Primary from \"../layouts/Primary\";\nimport AllianceBar from \"../components/AllianceBar\";\n\nimport \"./Bracket.css\";\nimport {\n    AllianceDescription,\n    BasicAllianceDescription,\n    BracketHalf, SeriesAllianceDescription, SeriesAllianceState, Sponsor,\n} from \"../../javaTypes\";\nimport {useRef, useState, useContext, useEffect, ReactNode, useMemo} from \"react\";\nimport { t } from \"../../i18n\";\nimport { RankingData } from \"../rankingDataState\";\nimport DisplayParams, {DisplayParamsType, DivisionInfo} from \"../DisplayParams\";\nimport {isBasicAllianceDescription, isDoubleElimsSeriesDescription, isSeriesAllianceDescription} from \"../util\";\nimport {Timeout} from \"../displayContainer\";\nimport MiniTimeout, {MiniTimeoutType} from \"../layouts/MiniTimeout\";\nimport {DisplayConfig} from \"../Config\";\nimport {useInterval} from \"../../hooks\";\nimport FormattedSeed from \"../components/FormattedSeed\";\nimport EventLogo from \"../components/EventLogo\";\n\nexport default function Bracket({config, showDivisionLabel} : {config: DisplayConfig, showDivisionLabel?: boolean}) {\n    const params = useContext(DisplayParams)\n    const divisionNumber = params.divisionNumber || 0\n    const bracketContainer = useRef<HTMLDivElement | null>(null);\n    const state = useContext(RankingData);\n    const timeout = useContext(Timeout);\n\n    const bracket = state[divisionNumber]?.bracket;\n    const structure = state[divisionNumber]?.playoffStructure;\n    // Resort the tournament in a logical display order so that schedule-driven swaps don't create criss-crossing\n    // lines on the bracket\n    const sortedSeries = useMemo(() => {\n        return structure ? [...structure.series].sort((a, b) => {\n            if(a.round != b.round) {\n                return a.round - b.round;\n            }\n            if(isDoubleElimsSeriesDescription(a) && isDoubleElimsSeriesDescription(b) && a.bracketHalf != b.bracketHalf) {\n                return a.bracketHalf == 'LOWER' ? 1 : -1;\n            }\n            const aWinners: number[] = [isSeriesAllianceDescription(a.red) ? a.red.series : null, isSeriesAllianceDescription(a.blue) ? a.blue.series : null].filter(m => m != null) as number[]\n            const bWinners: number[] = [isSeriesAllianceDescription(b.red) ? b.red.series : null, isSeriesAllianceDescription(b.blue) ? b.blue.series : null].filter(m => m != null) as number[]\n            // If both series have any winners, sort by the series numbers of those winners\n            const firstWinner = Math.min(...aWinners, ...bWinners, 999)\n            if(firstWinner < 999) {\n                return aWinners.includes(firstWinner) ? -1 : 1\n            }\n            return a.series - b.series\n        }) : undefined\n    }, [structure]);\n\n    const maxLayers = Math.max(...(sortedSeries?.filter(p => bracket?.seriesData[p.series - 1]).map(p => p.round) || [0]))\n    let maxBasicLayer = Math.max(...(sortedSeries?.filter(p => isBasicAllianceDescription(p.red) || isBasicAllianceDescription(p.blue)).map(p => p.round) || [0]))\n    const maxInRoundHalf = Math.max(...(sortedSeries ? Object.values(sortedSeries.reduce((acc, p) => {\n        acc[`${p.round}-${isDoubleElimsSeriesDescription(p) ? p.bracketHalf : 'CENTER'}`] ||= 0\n        acc[`${p.round}-${isDoubleElimsSeriesDescription(p) ? p.bracketHalf : 'CENTER'}`]++\n        return acc\n    }, {} as Record<string, number>)) : [1]))\n    const columnGap = maxLayers == 7 ? 30 : 300 / (maxLayers - 1);\n    const [svgPaths, setSvgPaths] = useState<ReactNode>();\n\n    // TODO this should be useLayoutEffect\n    useEffect(() => {\n        if(!bracketContainer.current || !sortedSeries) {\n            setSvgPaths([])\n            return;\n        }\n\n        const scale = parseFloat(getComputedStyle(bracketContainer.current).getPropertyValue(\"--display-scale\"));\n        const baseRect = bracketContainer.current.getBoundingClientRect();\n\n\n        for(let i = 0; i < sortedSeries.length; i++) {\n            const pairing = sortedSeries[i];\n            const element = bracketContainer.current.getElementsByClassName(`series-${pairing.series}`)[0] as HTMLDivElement;\n\n            if(!element) {\n                continue;\n            }\n            element.style.marginTop = '0';\n        }\n\n        for(let i = 0; i < sortedSeries.length; i++) {\n            const pairing = sortedSeries[i];\n            const element = bracketContainer.current.getElementsByClassName(`series-${pairing.series}`)[0] as HTMLDivElement;\n            if(!element) {\n                continue;\n            }\n            const winnerMatch = sortedSeries.find(p =>\n                isSeriesAllianceDescription(p.red) && p.red.state == SeriesAllianceState.WINNER && p.red.series == pairing.series ||\n                isSeriesAllianceDescription(p.blue) && p.blue.state == SeriesAllianceState.WINNER && p.blue.series == pairing.series)\n            if(!winnerMatch) {\n                continue;\n            }\n\n            const bars = element.getElementsByClassName(\"allianceBar\");\n            const upperRect = bars[0].getBoundingClientRect();\n            const lowerRect = bars[1].getBoundingClientRect();\n\n            const winnerBarsContainer = bracketContainer.current.getElementsByClassName(`series-${winnerMatch.series}`)[0]\n            if(!winnerBarsContainer) {\n                continue;\n            }\n\n            const winnerBars = winnerBarsContainer.getElementsByClassName(\"allianceBar\");\n            const winnerBar = isSeriesAllianceDescription(winnerMatch.red) && winnerMatch.red.state == SeriesAllianceState.WINNER && winnerMatch.red.series == pairing.series ? winnerBars[0] : winnerBars[1]\n            const winnerBarRect = winnerBar.getBoundingClientRect();\n\n            const myStyles = getComputedStyle(element)\n            const winnerStyles = getComputedStyle(winnerBarsContainer)\n            if(winnerStyles.gridRowStart == myStyles.gridRowStart && winnerStyles.gridRowEnd == myStyles.gridRowEnd) {\n                // Do an offset to make things line up pretty\n                const centerWinnerBar = (winnerBarRect.y + winnerBarRect.height / 2) / scale\n                const centerUpperBar = (upperRect.y + upperRect.height / 2) / scale\n                const centerLowerBar = (lowerRect.y + lowerRect.height / 2) / scale\n                const offset = centerWinnerBar - (centerUpperBar + centerLowerBar) / 2\n                // Offset must be doubled because vertical centering means effectively only half the offset is applied directly\n                element.style.marginTop = offset * 2 + \"px\"\n            }\n        }\n        const paths: ReactNode[] = []\n        for(let i = 0; i < sortedSeries.length; i++) {\n            const pairing = sortedSeries[i];\n            const element = bracketContainer.current.getElementsByClassName(`series-${pairing.series}`)[0];\n            if(!element) {\n                continue;\n            }\n            const winnerMatch = sortedSeries.find(p =>\n                isSeriesAllianceDescription(p.red) && p.red.state == SeriesAllianceState.WINNER && p.red.series == pairing.series ||\n                isSeriesAllianceDescription(p.blue) && p.blue.state == SeriesAllianceState.WINNER && p.blue.series == pairing.series)\n            if(!winnerMatch) {\n                continue;\n            }\n\n            const bars = element.getElementsByClassName(\"allianceBar\");\n            const upperRect = bars[0].getBoundingClientRect();\n            const lowerRect = bars[1].getBoundingClientRect();\n\n            const winnerBarsContainer = bracketContainer.current.getElementsByClassName(`series-${winnerMatch.series}`)[0]\n            if(!winnerBarsContainer) {\n                continue;\n            }\n            const upperBarStart = { x: (upperRect.x + upperRect.width - baseRect.x) / scale, y: (upperRect.y + upperRect.height / 2 - baseRect.y) / scale };\n            const lowerBarStart = { x: (lowerRect.x + lowerRect.width - baseRect.x) / scale, y: (lowerRect.y + lowerRect.height / 2 - baseRect.y) / scale };\n            paths.push(<path key={`bridge-${pairing.series}`} d={`M ${upperBarStart.x} ${upperBarStart.y} h ${columnGap/4} V ${lowerBarStart.y} h -${columnGap/4}`} stroke=\"white\" fill=\"transparent\" strokeWidth=\"4\"/>)\n\n            const winnerBars = winnerBarsContainer.getElementsByClassName(\"allianceBar\");\n            const winnerBar = isSeriesAllianceDescription(winnerMatch.red) && winnerMatch.red.state == SeriesAllianceState.WINNER && winnerMatch.red.series == pairing.series ? winnerBars[0] : winnerBars[1]\n            const winnerBarRect = winnerBar.getBoundingClientRect();\n            const winnerBarStart = { x: (winnerBarRect.x - baseRect.x) / scale, y: (winnerBarRect.y + winnerBarRect.height / 2 - baseRect.y) / scale };\n\n            paths.push(<path key={`line-${pairing.series}`} d={`M ${upperBarStart.x + columnGap/4} ${(upperBarStart.y + lowerBarStart.y) / 2} h ${columnGap * (1 / 3)} V ${winnerBarStart.y} H ${winnerBarStart.x}`} stroke=\"white\" fill=\"transparent\" strokeWidth=\"4\"/>)\n        }\n        setSvgPaths(paths)\n    }, [sortedSeries, bracketContainer.current]);\n\n    const firstUnplayed = Math.min(...(sortedSeries?.filter(p => {\n        const layer = bracket?.seriesData[p.series - 1]\n        const winnerExists = ((layer?.series?.red.wins || 0) >= p.matchesToWin || (layer?.series?.blue.wins || 0) >= p.matchesToWin)\n\n        return !winnerExists\n    }).map(p => p.series) || [0]))\n\n    let allianceMap = new Map();\n    const last = sortedSeries ? sortedSeries[sortedSeries.length - 1] : undefined;\n    let hasExtraMatch = last && last.series && isDoubleElimsSeriesDescription(last) && last.isFinals && last.optional && !!bracket?.seriesData[last.series - 1];\n    for (let b of bracket?.seriesData ?? []) {\n        if (b?.series?.red && b?.series?.red.seed) {\n            allianceMap.set(parseInt(b.series?.red.seed), b?.series?.red?.teams);\n        }\n        if (b?.series?.blue && b?.series?.blue.seed) {\n            allianceMap.set(parseInt(b.series?.blue.seed), b?.series?.blue?.teams);\n        }\n    }\n    const maxTeams = Math.max(...[...allianceMap.values()].map(x => x.length));\n    const hasByes = maxBasicLayer > 1\n    const isDoubleElims = sortedSeries?.some(isDoubleElimsSeriesDescription)\n    let cols = \"repeat(${maxBasicLayer}, ${hasByes || maxInRoundHalf < 3 || !isDoubleElims ? 2 : 3}fr)\"\n    if(maxLayers - maxBasicLayer > 0) {\n        cols = `${cols} repeat(${maxLayers - maxBasicLayer}, minmax(0, 1fr))`\n    }\n    const alliances = sortedSeries?.flatMap(p => {\n        const ret = []\n        if(isBasicAllianceDescription(p.red)) {\n            ret.push(p.red.alliance)\n        }\n        if(isBasicAllianceDescription(p.blue)) {\n            ret.push(p.blue.alliance)\n        }\n        return ret\n    }).sort((a, b) => a - b) || []\n    const eliminatedAlliances: number[] = []\n    for(const p of sortedSeries || []) {\n        const layer = bracket?.seriesData[p.series - 1]\n        const redWon = (layer?.series?.red.wins || 0) >= p.matchesToWin\n        const blueWon = (layer?.series?.blue.wins || 0) >= p.matchesToWin\n        const winnerExists = redWon || blueWon\n        // A team is eliminated in lower bracket matches or in the last finals match to be played\n        if(winnerExists && (!isDoubleElimsSeriesDescription(p) || p.bracketHalf == 'LOWER' || p == sortedSeries![bracket!.seriesData.length - 1])) {\n            if(redWon) {\n                console.log(`pushing ${layer!.series.red.alliance} as eliminated from series ${p.series}`)\n                eliminatedAlliances.push(layer!.series.blue.alliance)\n            } else if(blueWon) {\n                console.log(`pushing ${layer!.series.blue.alliance} as eliminated from series ${p.series}`)\n                eliminatedAlliances.push(layer!.series.red.alliance)\n            }\n        }\n    }\n\n    return <Primary qr={\"/playoffs\"} alternateCompact>\n        {config.type !== \"pit\" ? <MiniTimeout type={MiniTimeoutType.TOP_RIGHT} config={config} timeout={timeout}/> : null}\n        {showDivisionLabel ? <div className=\"bracketDivisionLabel\"><EventLogo logo={params.eventLogo} />{params.eventName.replace(\" Division\", \"\")}</div> : null}\n        <div className={`bracket ${alliances.length > 6 ? 'shorter' : ''}`} style={{\n            gridTemplateColumns: cols,\n            gridTemplateRows: isDoubleElims ? `repeat(${maxInRoundHalf}, minmax(0, ${hasByes ? 3 : 2}fr)) repeat(${maxInRoundHalf}, minmax(0, ${hasByes ? 2 : 1}fr))` : `repeat(${maxInRoundHalf}, minmax(0, 1fr))`,\n            columnGap: columnGap + \"px\"\n        }} ref={bracketContainer}>\n            {sortedSeries?.map((p, seriesIndex) => {\n                const sectionElements = sortedSeries.filter(r => r.round == p.round && (!isDoubleElimsSeriesDescription(p) || !isDoubleElimsSeriesDescription(r) || p.bracketHalf == r.bracketHalf));\n                const maxHeight = sectionElements.length == 1 && sectionElements[0].isFinals && isDoubleElims ? maxInRoundHalf * 2 : maxInRoundHalf\n                const height = maxHeight / sectionElements.length;\n                const slotInRound = sectionElements.indexOf(p)\n                const halfOfRound = (isDoubleElimsSeriesDescription(p) ? p.bracketHalf == 'LOWER' : false) ? maxInRoundHalf + 1: 1\n                const layer = bracket?.seriesData[p.series - 1]\n\n                if(isDoubleElimsSeriesDescription(p) && p.optional && !layer) {\n                    return null\n                }\n\n                const redWon = (layer?.series?.red.wins || 0) >= p.matchesToWin\n                const blueWon = (layer?.series?.blue.wins || 0) >= p.matchesToWin\n                const winnerExists = redWon || blueWon\n                const hasTie = isDoubleElimsSeriesDescription(p) && p.matchesToWin === 1 && layer?.series?.ties;\n                const col = isDoubleElimsSeriesDescription(p) ? layer?.series : 0;\n                let matchSuffix = \"\";\n                if (hasTie) {\n                    matchSuffix = ` +${layer?.series?.ties}`;\n                }\n\n                return <div key={p.series}\n                  className={`allianceGroup series-${p.series} ${p.series == firstUnplayed ? 'current' : ''}`}\n                  style={{\n                      gridColumnStart: p.round,\n                      gridColumnEnd: \"span 1\",\n                      gridRowStart: halfOfRound + (slotInRound * height),\n                      gridRowEnd: halfOfRound + ((slotInRound + 1) * height)\n                  }}>\n                    {/* If the series name starts with playoff, that is implied for the bracket */}\n                    <span className=\"allianceGroupLabel\">{p.name.replace(/^Playoff /, '')}<span style={{fontSize: '30px', position: 'relative', top: '-2px'}}>{matchSuffix}</span></span>\n                    <AllianceBar vertical={maxInRoundHalf < 3 && maxLayers < 6} teamCount={1}\n                                 color={\"red\"} al