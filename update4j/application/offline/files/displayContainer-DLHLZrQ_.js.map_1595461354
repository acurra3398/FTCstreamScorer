an\n}) {\n    if(!alliance) return null;\n    let teams = !alliance.teams ? [] : [...alliance.teams];\n    let side = hflip ? (color === \"red\" ? \"right\" : \"left\") : (color === \"red\" ? \"left\" : \"right\");\n    let items = teams.map((team, i) =>\n        team.number === \"\" ? null : <div key={i} id={`${color}${i + 1}`} className={`teamNumber team${i + 1} ${color}Number ${team.carriesCard ? \"carriesCard\" : \"\"}`}>{team.number}</div>\n    );\n    return <div id={`${color}Numbers`} className={`teamNumbers ${side} number-count-${teams.length} ${wingSpan} ${isParent ? \"parent\" : \"\"}`}>\n        {showSeed && alliance.seed ? <span className={\"seed\"}>\n            <FormattedSeed seed={alliance.seed} compact />\n        </span> : null}\n        {items}\n    </div>\n}\n\nfunction isEmptyCommand(command: DisplayCommand<any>) : command is DisplayCommand<{ _empty: any }> {\n    return '_empty' in command.params\n}\n\nexport default function Match({config, command, nowCallback, scoreUpdate, keyPanel, audienceKey, timerOnly} : {\n    config: DisplayConfig,\n    command: DisplayCommand<ShowMatch> | DisplayCommand<{ _empty: any }>,\n    nowCallback: () => number,\n    scoreUpdate?: DisplayCommand<ScoreUpdate>,\n    keyPanel?: 'auto' | 'teleop' | 'shared' | 'all',\n    ref?: () => void,\n    audienceKey?: ReactNode,\n    timerOnly?: boolean\n}) {\n    const displayParams = useContext(DisplayParams);\n    const style = timerOnly ? 'timerOnly' : displayParams.style\n    const isEmpty = isEmptyCommand(command);\n    const isSingleTeam = !isEmpty && command.params.singleTeam;\n    const isPlayoff = !isEmpty && command.params.tournamentLevel == TournamentLevel.PLAYOFF;\n    const { remaining, running } = useTimer({\n        startAt: command.type === 'START_MATCH' ? command.ts : null,\n        duration: command.type === 'ABORT_MATCH' ? 0 : 120 + 30 + 8 + (isSingleTeam ? 3 : 0),\n        nowCallback: nowCallback,\n        sounds: config.mute ? {} : (\n            {\n                [120 + 30 + 8 + 3]: 'countdown',\n                [120 + 30 + 8]: 'matchStart',\n                [120 + 8]: 'autoEnd',\n                [120 + 6]: 'controllers',\n                [120 + 3]: 'countdown',\n                [120]: 'startTeleop',\n                [20]: 'endgameWarning',\n                [0]: 'matchEnd'\n            })\n    })\n    useEffect(() => {\n      if(command.type === 'ABORT_MATCH' && nowCallback() - command.ts < 1000) {\n          play('abortMatch')\n      }\n    }, [command.ts])\n    let displayTimeRemaining = remaining;\n    let icon : 'robot' | 'gamepad' | 'hand' | null = null;\n    let period : 'auto' | 'teleop' | 'auto_transition' | 'endgame' | 'review' | null = null;\n    let ScoreElement = Score;\n    if(running) {\n        if (remaining > 158) {\n            displayTimeRemaining = 150;\n        } else if (remaining > 128) {\n            displayTimeRemaining = remaining - 8;\n            period = 'auto';\n            icon = 'robot';\n            ScoreElement = Score;\n        } else if (remaining > 120) {\n            displayTimeRemaining = remaining - 120;\n            period = 'auto_transition';\n            icon = 'hand';\n            ScoreElement = Score;\n        } else if (remaining > 30) {\n            period = 'teleop';\n            icon = 'gamepad';\n        } else if (remaining > 0) {\n            period = 'endgame';\n            icon = 'gamepad';\n        }\n    } else if(command.type === 'ABORT_MATCH') {\n        displayTimeRemaining = 0;\n    } else if(command.type === 'SHOW_MATCH') {\n        displayTimeRemaining = 120 + 30\n        ScoreElement = Score;\n    } else if (remaining === 0) {\n        period = 'review';\n    }\n    const vflip = config.scoringBarLocation === 'top';\n    const hflip = config.allianceOrientation === 'flipped';\n\n    /*\n        Layouts:\n        AD (scoring elements in 2/3, medium-timer in 1/3 ) [vflip & hflip]\n        FD (big timer in 2/3, scoring elements in 1/3) [vflip & hflip]\n        overlay (scoring elements in 1/3, chroma rest) [vflip & hflip]\n        no-live (Primary w/ big timer with wings) [hflip]\n        no-live overlay (medium timer in 1/3, chroma rest) [vflip & hflip]\n     */\n    if (!isEmpty && displayParams.liveDisabled && !config.overlay) {\n        let wings = ([\"red\", \"blue\"] as const).map(a => <WingSet key={a} color={a} alliance={command.params[a]} hflip={hflip} wingSpan=\"jumbo\" teamCount={Math.max(command.params.red.teams.length, command.params.blue.teams.length)}/>);\n        return <Primary>\n            <div className=\"timer\">\n                {renderTime(displayTimeRemaining)}\n                {icon ? <i className={`fas fa-${icon}`} /> : null }\n            </div>\n            {wings}\n        </Primary>\n    } else if(style === 'timerOnly') {\n        return <Primary>\n            <div className=\"timer\">\n                {renderTime(displayTimeRemaining)}\n                {icon ? <i className={`fas fa-${icon}`} /> : null }\n            </div>\n        </Primary>\n    }\n    let twoThirds;\n    const oneThirdTimer = (isSingleTeam ? config.overlay : !config.fieldStyleTimer) || displayParams.liveDisabled; // if the total scores are touching\n    const redScore = scoreUpdate ? (isSingleTeam ? scoreUpdate.params.redScores : scoreUpdate.params.redScores) : {}\n    const blueScore = scoreUpdate ? (isSingleTeam ? scoreUpdate.params.redScores : scoreUpdate.params.blueScores) : {}\n    let oneThirdScores = false;\n    const showHrReviewBanner = period === 'review' && scoreUpdate?.params.hrReview;\n    if (config.type == \"cmpBar\") {\n        const redSection = <div className={\"matchInfo red\"}>\n            <TotalScore color={\"red\"} score={redScore} otherScore={blueScore} isAuto={period?.startsWith('auto')} />\n            {isEmpty ? null : <WingSet color={\"red\"} alliance={command.params.red} showSeed isParent={displayParams.isDualDivisionParent} teamCount={Math.max(command.params.red.teams.length, command.params.blue.teams.length)}/>}\n        </div>\n        const blueSection = <div className={\"matchInfo blue\"}>\n            <TotalScore color={\"blue\"} score={blueScore} otherScore={redScore} isAuto={period?.startsWith('auto')} />\n            {isEmpty ? null : <WingSet color={\"red\"} alliance={command.params.blue} showSeed isParent={displayParams.isDualDivisionParent} teamCount={Math.max(command.params.red.teams.length, command.params.blue.teams.length)}/>}\n        </div>\n        return <CmpBar timer={renderTime(displayTimeRemaining)} pageTitle={isEmpty ? \"\" : <><EventLogo logo={displayParams.matchPrefixLogo} /> {truncateMatchNameForCmpBar(command.params.matchName)}</>}>\n            <div className=\"matchFull\">\n                {hflip ? [blueSection, redSection] : [redSection, blueSection]}\n            </div>\n        </CmpBar>\n    } else if (config.overlay) {\n        twoThirds = <div className={`chroma ${vflip ? \"vflip\" : \"\"}`} style={{backgroundColor: `${config.overlayColor}`}}/>;\n        if(!displayParams.liveDisabled) {\n            oneThirdScores = true;\n        }\n    } else if (config.fieldStyleTimer) {\n        twoThirds = <TwoThirds vflip={vflip}>\n            <div id=\"fieldHeader\">{isSingleTeam ? t`Match Timer` : (!isEmpty && command.params.field > 0 ? t`Field ${command.params.field}` : '')}</div>\n            <div className=\"timer\">\n                {renderTime(displayTimeRemaining)}\n                {icon ? <i className={`fas fa-${icon}`} /> : <i className={`fas`}>&nbsp;</i> }\n                {scoreUpdate ? <RandomizationArtifacts number={scoreUpdate.params.rand as 1 | 2 | 3} /> : null}\n            </div>\n            {showHrReviewBanner ? <div className={\"reviewBanner\"}>{t`Match Under Review`}</div> : null}\n        </TwoThirds>;\n        oneThirdScores = !displayParams.liveDisabled;\n    } else {\n        if (isSingleTeam) {\n            twoThirds = <TwoThirds vflip={vflip}>\n                <FullScore color={\"orange\"} score={redScore} otherScore={{}} isSingleTeam\n                           currentPeriod={period} keyPanel={keyPanel} isPlayoff={isPlayoff} />\n                <div className=\"smallTimer\">\n                    <div className=\"timer\">\n                        {renderTime(displayTimeRemaining)}\n                        {icon ? <i className={`fas fa-${icon}`} /> : null }\n                    </div>\n                </div>\n            </TwoThirds>\n        } else {\n            const redFullScores = <FullScore color={\"red\"} score={redScore} otherScore={blueScore}\n                                             currentPeriod={period} right={hflip} keyPanel={keyPanel} hrReview={showHrReviewBanner} isPlayoff={isPlayoff} />;\n            const blueFullScores = <FullScore color={\"blue\"} score={blueScore} otherScore={redScore}\n                                              currentPeriod={period} right={!hflip} keyPanel={keyPanel} hrReview={showHrReviewBanner} isPlayoff={isPlayoff} />;\n            twoThirds = <TwoThirds vflip={vflip}>\n                <div className={\"d-flex h-100\"}>\n                    {hflip ? [blueFullScores, redFullScores] : [redFullScores, blueFullScores]}\n                </div>\n            </TwoThirds>;\n        }\n    }\n\n\n    return <div className={`h-100 w-100 ${isSingleTeam ? 'singleTeam' : ''} matchPage`}>\n        {twoThirds}\n        <OneThird matchName={isEmpty ? '' : <><EventLogo logo={displayParams.matchPrefixLogo} /> {command.params.matchName}</>} vflip={vflip}>\n            {!isEmpty ? (isSingleTeam ? [] : ['red', 'blue'] as const).map(a => <WingSet key={a} color={a} alliance={command.params[a]} hflip={hflip} wingSpan={!displayParams.liveDisabled ? (config.overlay || !config.fieldStyleTimer ? \"overlay\" : \"standard\") : \"compact\"} teamCount={Math.max(command.params.red.teams.length, command.params.blue.teams.length)}/>) : null}\n            {!displayParams.liveDisabled ?\n                <>\n                    <div className={`scor