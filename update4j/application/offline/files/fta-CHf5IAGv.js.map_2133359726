{"version":3,"file":"fta-CHf5IAGv.js","sources":["../../../../../src/main/javascript/fta/MatchView.jsx","../../../../../src/main/javascript/fta/IssueItem.jsx","../../../../../src/main/javascript/fta/TeamView.jsx","../../../../../src/main/javascript/fta/ListView.jsx","../../../../../src/main/javascript/fta/NoteDialog.jsx","../../../../../src/main/javascript/fta/FTANotepad.jsx","../../../../../src/main/javascript/fta/index.jsx"],"sourcesContent":["import \"./MatchView.css\";\n\nfunction TeamSlot({match, team, alliance, issues, hidden, openTeam, openNote}) {\n    if(!team) {\n        team = {};\n    }\n    let teamIssues = issues.get(team.number) || [];\n    let active = teamIssues.reduce((n, e) => !e.resolved ? n + 1 : n, 0);\n    let resolved = teamIssues.reduce((n, e) => e.resolved ? n + 1 : n, 0);\n    return hidden ? null : <div className={`matchTeam ${alliance}`}>\n        <div className={\"teamNumber\"} onClick={() => openTeam(team.number)}>{team.displayNumber}</div>\n        {team.number ? <div className={\"issueGroup\"}>\n            <div className={\"active issueCount\"}>A: {active}</div>\n            <div className={\"resolved issueCount\"}>R: {resolved}</div>\n            <button className={\"newIssue\"} onClick={() => openNote(match, team.number)}><i className={\"fa fa-plus-circle fa-4\"}/></button>\n        </div> : null}\n    </div>\n}\n\nexport default function MatchView({match, matchList, issues, setCurrentMatch, openTeam, saveNoteCallback, openNote}) {\n    let matchIndex = match ? matchList.findIndex(m => m.shortName === match) : -1;\n    let matchObj = matchIndex >= 0 ? matchList[matchIndex] : null;\n    let r3 = matchObj?.red.team3 || matchObj?.blue.team3;\n    let teams = [1, 2, 3].map(r => ['red', 'blue'].map(a => <TeamSlot match={match} team={matchObj?.[a][\"team\" + r]?.data} alliance={a} key={`${a}_${r}`} issues={issues} hidden={r === 3 && !r3} openTeam={openTeam} openNote={openNote}/>));\n    let matches = matchList ? matchList.map(m => <button className={\"dropdown-item\"} key={m.shortName} onClick={() => setCurrentMatch(m.shortName)}>{m.shortName}</button>) : [];\n\n    return <div className={\"matchView\"}>\n        <br/>\n        <div className={\"matchHeader\"}>\n            <button className={\"matchLeft btn btn-secondary\"} disabled={matchIndex <= 0} onClick={() => setCurrentMatch(matchList[matchIndex - 1].shortName)}><i className={\"fa fa-arrow-left\"}/></button>\n            <span className={\"matchName h4\"}>{t`Match ${match}`}</span>\n            <button className={\"btn btn-secondary btn-sm dropdown-toggle\"} data-toggle=\"dropdown\" aria-haspopup=\"true\" aria-expanded=\"false\"/>\n            <div className=\"dropdown-menu\" aria-labelledby=\"matchSearch\">\n                {matches}\n            </div>\n            <button className={\"btn btn-secondary\"} disabled={matchIndex < 0 || matchIndex >= matchList.length - 1} onClick={() => setCurrentMatch(matchList[matchIndex + 1].shortName)}><i className={\"fa fa-arrow-right\"}/></button>\n        </div>\n        <div className={\"matchInfo\"}>\n            Field: {matchObj?.field}\n        </div>\n        <div className={\"matchTeams\"}>\n            {teams}\n        </div>\n    </div>\n}","import './IssueItem.css';\n\nexport default function IssueItem({issue, saveNoteCallback, label, openNote, teams}) {\n    const TYPE_MAP = {\n        DS_PHONE: t`DS Issue (Phone)`,\n        DS_HUB: t`DS Issue (Hub)`,\n        RC_PHONE: t`RC Issue (Phone)`,\n        RC_HUB: t`RC Issue (Hub)`,\n        NO_ROBOT: t`No Robot`,\n        CONFIG: t`Configuration Issue`,\n        POWER: t`Robot Power Issue`,\n        CONNECTION: t`Connection Issue`,\n        ELECTRICAL: t`Electrical Issue`,\n        OTHER_ROBOT: t`Other Robot Issue`,\n        INSPECTION: t`Inspection Issue`,\n        OTHER: t`Other Issue`\n    }\n    const onclick = (e) => {\n        e.stopPropagation();\n        issue.resolved = !issue.resolved;\n        saveNoteCallback(issue);\n    }\n    return <div className={`teamIssue teamIssueListItem ${issue.resolved ? \"resolved\" : \"active\"}`} onClick={() => openNote(issue)}>\n        <div className={\"teamIssueItemLeft\"}>\n            <div className={\"teamIssueItemTop\"}>\n                <span className={\"teamIssueMatch\"}>{label === 'team' ? teams.find(x => x.data.number === issue.team)?.data.displayNumber : issue.match}</span>\n                <div className={\"teamIssueType\"}>{TYPE_MAP[issue.type]}</div>\n            </div>\n            <div className={\"teamIssueSummary\"}>{issue.summary}</div>\n        </div>\n        <button onClick={onclick}><i className={`fa-2x ${issue.resolved ? \"fa fa-check-circle\" : \"far fa-circle\"}`}/></button>\n    </div>\n}","import \"./TeamView.css\";\nimport { t } from 'ftc-common/i18n';\nimport IssueItem from \"./IssueItem\";\nimport {matchSortShortname} from \"./FTANotepad\";\n\nexport default function TeamView({team, displayNumber, teamName, issues, matchList, currentMatch, openMatch, saveNoteCallback, openNote}) {\n    const hasTeam = (m, t) => {\n        for (let a of ['red', 'blue']) {\n            for (let r of [1,2,3]) {\n                if (m[a][\"team\" + r]?.data.number === t) {\n                    return true;\n                }\n            }\n        }\n        return false;\n    }\n    const toMatchItem = (m) => <div className={`teamMatch teamMatchListItem ${currentMatch === m.shortName ? \"current\" : \"\"}`} key={m.shortName} onClick={() => openMatch(m.shortName)}>{m.shortName}</div>\n    const hasMe = matchList.filter(x => hasTeam(x, team));\n    const canAddIssue = hasMe.map(x => x.shortName === currentMatch).includes(true);\n    const completed = hasMe.filter(x => x.state === 'COMMITTED').map(toMatchItem);\n    const future = hasMe.filter(x => x.state !== 'COMMITTED').map(toMatchItem);\n    const sorted = issues?.sort((a,b) => matchSortShortname(a.match, b.match));\n    const active = sorted?.filter(x => !x.resolved).map(x => <IssueItem issue={x} saveNoteCallback={saveNoteCallback} openNote={openNote} key={x.id}/>);\n    const resolved = sorted?.filter(x => x.resolved).map(x => <IssueItem issue={x} saveNoteCallback={saveNoteCallback} openNote={openNote} key={x.id}/>);\n    return <div className={\"teamView\"}><br/>\n        <div className={\"teamHeader\"}>\n            <div className={\"teamNumber h4\"}>{t`Team #${displayNumber}`}</div>\n            <div className={\"teamName\"}>{teamName}</div>\n        </div>\n        <div className={\"teamBody\"}>\n            <div style={{width: \"75%\", borderRight: \"1px solid black\"}}>\n                <div style={{margin: \".5em\", minHeight: \"300px\"}}>\n                    <div className={\"teamIssueListHeader teamIssueListItem active\"}>{t`ACTIVE:`}</div>\n                    {active}\n                    <div className={\"teamIssueListHeader teamIssueListItem resolved\"}>{t`RESOLVED:`}</div>\n                    {resolved}\n                </div>\n                {canAddIssue ? <div style={{textAlign: \"center\", margin: \".3em\"}}><button style={{width: \"50%\"}} className={\"btn btn-sm btn-dark\"} onClick={() => openNote({match: currentMatch, team: team})}>{t`Add Match Issue`}</button></div> : null}\n                <div style={{textAlign: \"center\", margin: \".3em\"}}><button style={{width: \"50%\"}} className={\"btn btn-sm btn-dark\"} onClick={() => openNote({match: 'G', team: team})}>{t`Add General Issue`}</button></div>\n            </div>\n            <div style={{width: \"25%\", textAlign: \"center\"}}>\n                <div style={{fontWeight: 'bold'}}>Matches</div>\n                <div className={\"teamMatchListHeader teamMatchListItem\"}>{t`COMPLETE:`}</div>\n                {completed}\n                <div className={\"teamMatchListHeader teamMatchListItem\"}>{t`FUTURE:`}</div>\n                {future}\n            </div>\n        </div>\n    </div>\n}","import './ListView.css';\nimport { t } from 'ftc-common/i18n';\nimport IssueItem from \"./IssueItem\";\nimport {useState} from \"react\";\nimport {matchSortShortname} from \"./FTANotepad\";\n\nfunction renderByTeam(issues, teams, filter, saveNoteCallback, openNote) {\n    const content = [];\n    const keys = Array.from(issues.keys()).sort((a,b) => a - b);\n    for (let team of keys) {\n        let teamIssues = issues.get(team).filter(x => filter !== 'active' || !x.resolved);\n        if (teamIssues.length) {\n            content.push(<div className={\"teamIssueListHeader teamIssueListItem\"} key={team}>{teams.find(x => x.data.number === team)?.data.displayNumber} - {teams.find(x => x.data.number === team)?.data.name}</div>);\n            for (let issue of teamIssues.sort((a,b) => matchSortShortname(a.match, b.match))) {\n                content.push(<IssueItem issue={issue} saveNoteCallback={saveNoteCallback} openNote={openNote} key={`${issue.team}_${issue.id}`}/>)\n            }\n        }\n    }\n    return content;\n}\n\nfunction renderByMatch(issues, teams, filter, saveNoteCallback, openNote) {\n    const content = [];\n    const byMatch = new Map();\n    const keys = Array.from(issues.keys()).sort((a,b) => a - b);\n    for (let team of keys) {\n        for (let issue of issues.get(team).filter(x => filter !== 'active' || !x.resolved)) {\n            const matchIssues = byMatch.has(issue.match) ? byMatch.get(issue.match) : [];\n            matchIssues.push(issue);\n            byMatch.set(issue.match, matchIssues);\n        }\n    }\n    for (let match of Array.from(byMatch.keys()).sort(matchSortShortname)) {\n        content.push(<div className={\"teamIssueListHeader teamIssueListItem\"} key={match}>{match}</div>);\n        for (let issue of Array.from(byMatch.get(match)).sort((a,b) => a.team - b.team)) {\n            content.push(<IssueItem teams={teams} issue={issue} saveNoteCallback={saveNoteCallback} label={\"team\"} openNote={openNote} key={`${issue.team}_${issue.id}`}/>)\n        }\n    }\n    return content;\n}\nexport default function ListView({teams, issues, saveNoteCallback, openNote}) {\n    const [group, setGroup] = useState('team');\n    const [filter, setFilter] = useState('active');\n\n    const content = group === 'team' ? renderByTeam(issues, teams, filter, saveNoteCallback, openNote) : renderByMatch(issues, teams, filter, saveNoteCallback, openNote);\n\n    return <div><br/>\n        <div className={\"listHeader h6\"}>\n            <div>\n                {t`Group: `}\n                <button className={`btn btn-sm ${group === 'team' ? \"btn-secondary\" : \"btn-outline-secondary\"}`} onClick={() => setGroup('team')}>{t`Team`}</button>\n                <button className={`btn btn-sm ${group === 'match' ? \"btn-secondary\" : \"btn-outline-secondary\"}`} onClick={() => setGroup('match')} style={{marginLeft: \"1ch\"}}>{t`Match`}</button>\n            </div>\n            <div style={{marginLeft: 'auto'}}>\n                {t`Show: `}\n                <button className={`btn btn-sm ${filter === 'all' ? \"btn-secondary\" : \"btn-outline-secondary\"}`} onClick={() => setFilter('all')}>{t`All`}</button>\n                <button className={`btn btn-sm ${filter === 'active' ? \"btn-secondary\" : \"btn-outline-secondary\"}`} onClick={() => setFilter('active')} style={{marginLeft: \"1ch\"}}>{t`Active`}</button>\n            </div>\n        </div>\n        <div className={\"issueList\"}>\n            <div className={\"teamIssuesContainer\"}>\n                {content}\n            </div>\n        </div>\n    </div>\n}","import {Button, Modal, Form} from \"react-bootstrap\";\nimport {useId} from \"react\";\nimport {confirmDialog} from \"first-common/dialogs\";\n\nexport default function NoteDialog({show, setShow, note, setNote, saveNote, slackEnabled}) {\n    const sendToSlackId = useId();\n    const resolvedId = useId();\n    if (!note) {\n        return null;\n    }\n    function updateNote(changes) {\n        setNote(Object.assign({}, note, changes));\n    }\n\n    // check send to slack if it has already been sent to slack or if selected now (unless marking resolved)\n    let sendToSlackChecked = note.sendToSlack || note.slackId;\n    if (sendToSlackChecked && note.resolved && !note.slackId) {\n        // Don't check if resolved and not already sent\n        sendToSlackChecked = false;\n    }\n\n    return <Modal show={show} onHide={() => setShow(false)} size=\"lg\" className=\"noteModal\">\n        <Modal.Header closeButton>\n            <Modal.Title>{t`Team Issue`}</Modal.Title>\n        </Modal.Header>\n        <Modal.Body>\n            {note.match !== 'G' ? <><Form.Label>{t`Match: ${note.match}`}</Form.Label><br/></> : null}\n            <Form.Label>{t`Team: ${note.team}`}</Form.Label><br/>\n            <label htmlFor={\"noteTypeInput\"}>{t`Type:`}</label>\n            <select id={\"noteTypeInput\"} style={{marginLeft: \"10px\"}} value={note.type || 'OTHER'} onChange={(e) => updateNote({type: e.target.value})}>\n                <option value=\"DS_PHONE\">{t`DS Issue (Phone)`}</option>\n                <option value=\"DS_HUB\">{t`DS Issue (Hub)`}</option>\n                <option value=\"RC_PHONE\">{t`RC Issue (Phone)`}</option>\n                <option value=\"RC_HUB\">{t`RC Issue (Hub)`}</option>\n                <option value=\"NO_ROBOT\">{t`No Robot`}</option>\n                <option value=\"CONFIG\">{t`Configuration Issue`}</option>\n                <option value=\"POWER\">{t`Robot Power Issue`}</option>\n                <option value=\"CONNECTION\">{t`Connection Issue`}</option>\n                <option value=\"ELECTRICAL\">{t`Electrical Issue`}</option>\n                <option value=\"OTHER_ROBOT\">{t`Other Robot Issue`}</option>\n                <option value=\"INSPECTION\">{t`Inspection Issue`}</option>\n                <option value=\"OTHER\">{t`Other Issue`}</option>\n            </select><br/>\n            <Form.Label className={\"drop\"}>{t`Summary:`}</Form.Label>\n            <Form.Control type={\"text\"} value={note.summary || ''} onChange={(e) => updateNote({summary: e.target.value})}/>\n            <Form.Label className={\"drop\"}>{t`Details:`}</Form.Label>\n            <Form.Control as={\"textarea\"} value={note.details || ''} rows={4} style={{width: \"100%\"}} onChange={(e) => updateNote({details: e.target.value})}/><br/>\n\n            <Form.Check label={\"Resolved?\"} id={resolvedId} checked={note.resolved} onChange={(e) => updateNote({resolved: e.target.checked})}/>\n            {slackEnabled ? <Form.Check label={\"Send to slack?\"} id={sendToSlackId} checked={sendToSlackChecked} onChange={(e) => updateNote({sendToSlack: e.target.checked})} disabled={note.slackId || note.resolved}/> : null}\n\n        </Modal.Body>\n        <Modal.Footer>\n            <Button variant={\"danger\"} onClick={() => {\n                setShow(false);\n                confirmDialog({\n                    label: t`Confirm Note Delete`,\n                    text: t`This will permanently delete the note. Continue?`,\n                    onConfirm: () => {\n                        saveNote(Object.assign({}, note, {id: -note.id}));\n                        setShow(false);\n                    }\n                })\n            }} style={{marginRight: \"auto\"}} hidden={!note.id}>{t`Delete`}</Button>\n            <Button variant={\"secondary\"} onClick={() => setShow(false)}>{t`Cancel`}</Button>\n            <Button variant={\"primary\"} onClick={() => {\n                saveNote(note);\n                setShow(false);\n            }}>{t`Save`}</Button>\n        </Modal.Footer>\n    </Modal>\n}","import \"./FTANotepad.css\";\nimport {useEffect, useState} from \"react\";\nimport { t } from 'ftc-common/i18n';\nimport {usePersistentWebsocket} from \"ftc-common/utils\";\nimport MatchView from \"./MatchView\";\nimport TeamView from \"./TeamView\";\nimport ListView from \"./ListView\";\nimport NoteDialog from \"./NoteDialog\";\n\nfunction MatchToolbar({label, shortName, setCurrentMatch}) {\n    return <div className={\"matchBtn\"}>\n        <div>{label}</div>\n        {shortName ? <button className={\"btn btn-primary btn-sm\"} onClick={() => setCurrentMatch(shortName)}>{shortName}</button> : null}\n    </div>\n}\n\nexport function matchSortShortname(a, b) {\n    // Shortname is one of: G, Q#, M#, M#-#, F-#\n    // A team will never appear in more than 1 match series per round, so dont worry about intra-round sorting for tiebreakers\n    // TODO come back to this\n    if (!a || !a[0]) return 1;\n    if (!b || !b[0]) return -1;\n    if (a[0] === b[0]) {\n        const toNum = x => {\n            const i = x.indexOf('-');\n            if (i > 0) {\n                return parseInt(x.substring(1, i) + (parseInt(x.substring(i)) / 100));\n            }\n            return parseInt(x.substring(1));\n        }\n        return toNum(a) - toNum(b);\n    } else {\n        const levelMap = {G: 0, Q: 1, M: 2, S: 3, F: 4, I: 5};\n        const toLevel = (m) => levelMap[m[0]];\n        return toLevel(a) - toLevel(b);\n    }\n}\n\nexport default function FTANotepad({eventCode, slackEnabled}) {\n    const [view, setView] = useState('match');\n    const [lastUpdate, setLastUpdate] = useState(null);\n    const [matchList, setMatchList] = useState([]);\n    const [teamList, setTeamList] = useState([]);\n    const [currentTeam, setCurrentTeam] = useState(null);\n    const [currentMatch, setCurrentMatch] = useState(null);\n    const [activeMatch, setActiveMatch] = useState(null);\n    const [loadedMatch, setLoadedMatch] = useState(null);\n    const [issues, setIssues] = useState(new Map());\n    const [showNote, setShowNote] = useState(false);\n    const [activeNote, setActiveNote] = useState(null);\n    const { isConnected, send } = usePersistentWebsocket(`/stream/fta/?code=${eventCode}`, (data) => {\n        setLastUpdate(data);\n    });\n\n    useEffect(() => {\n        let data = lastUpdate;\n        if (!data) {\n            return;\n        }\n        const updateIssueState = (issues) => {\n            const initIssues = new Map();\n            for (let team in issues) {\n                initIssues.set(parseInt(team), issues[team]);\n            }\n            setIssues(initIssues);\n        }\n        if (data.type === \"INIT\") {\n            setMatchList(data.matches);\n            setTeamList(data.teams);\n            // reverse to find last loaded that meets criteria\n            const active = data.activeMatches.slice().reverse().find(m => [\"AUTO\", \"TELEOP\", \"REVIEW\", \"SUBMITTED\"].includes(m.state))?.shortName;\n            const loaded = data.activeMatches.slice().reverse().find(m => [\"UNPLAYED\", \"RANDOMIZED\"].includes(m.state))?.shortName;\n            const lastPlayed = data.matches.reduce((n, e) => e.start > n.start ? {start: e.start, match: e.shortName} : n, {start: 0, match: data.matches.length > 0 ? data.matches[0].shortName : null});\n            setActiveMatch(active);\n            setLoadedMatch(loaded === active ? null : loaded);\n            setCurrentMatch(active ? active : (loaded ? loaded : lastPlayed.match));\n            updateIssueState(data.issues);\n        } else if (data.type === \"MATCH_LIST\") {\n            setMatchList(data.matches);\n        } else if (data.type === \"LOAD\") {\n            setLoadedMatch(data.match);\n        } else if (data.type === \"START\") {\n            setActiveMatch(data.match);\n            if (loadedMatch === data.match) {\n                setLoadedMatch(null);\n            }\n        } else if (data.type === \"COMMIT\") {\n            const local = Array.from(matchList);\n            const localMatch = local.find(x => x.shortName === data.match);\n            if (localMatch) {\n                localMatch.state = 'COMMITTED'; // make sure it shows up in \"Completed\" section for teams\n                setMatchList(local);\n            }\n            if (data.match === activeMatch) {\n                setActiveMatch(null);\n            }\n            if (data.match === loadedMatch) {\n                setLoadedMatch(null);\n            }\n        } else if (data.type === \"ABORT\") {\n            if (data.match === activeMatch) {\n                setActiveMatch(null);\n                setLoadedMatch(data.match);\n            }\n        } else if (data.type === \"ISSUE\") { // pull this into its own useEffect that listens for issueList changes?\n            updateIssueState(data.issues);\n        } else {\n            console.log(data);\n        }\n    }, [lastUpdate, activeMatch, loadedMatch, matchList]);\n\n    const openTeam = (number) => {\n        setCurrentTeam(number);\n        setView('team');\n    }\n    const openMatch = (shortName) => {\n        setCurrentMatch(shortName);\n        setView('match');\n    }\n    const saveNote = (note) => {\n        if (!note.type) {\n            note.type = 'OTHER';\n        }\n        send(JSON.stringify({type: \"ISSUE\", issue: note}))\n    }\n    const createNote = (match, team) => {\n        setActiveNote({match: match, team: team});\n        setShowNote(true);\n    }\n    const openNote = (note) => {\n        setActiveNote(note);\n        setShowNote(true);\n    }\n\n    let content = null;\n    if (view === 'match') {\n        content = <MatchView match={currentMatch} matchList={matchList} issues={issues} setCurrentMatch={setCurrentMatch}\n                              openTeam={openTeam} openNote={createNote}/>;\n    } else if (view === 'team' ) {\n        content = <TeamView team={currentTeam} teamName={teamList.find(x => x.data.number === currentTeam)?.data.name}\n                             displayNumber={teamList.find(x => x.data.number === currentTeam)?.data.displayNumber}\n                             issues={issues.get(currentTeam)} currentMatch={currentMatch} matchList={matchList} openMatch={openMatch}\n                             saveNoteCallback={saveNote} openNote={openNote}/>;\n    } else if (view === 'list') {\n        content = <ListView teams={teamList} issues={issues} saveNoteCallback={saveNote} openNote={openNote}/>;\n    }\n    const leftToolbar = view === 'match' ? [\n            <MatchToolbar key='loaded' label={t`Loaded Match`} shortName={loadedMatch} setCurrentMatch={setCurrentMatch} />,\n            <MatchToolbar key='active' label={t`Active Match`} shortName={activeMatch} setCurrentMatch={setCurrentMatch} />\n        ] : <button className={\"btn btn-secondary toolbarBtn\"} onClick={() => setView('match')}>{t`Match View`}</button>;\n    const teams = teamList.map(t => <button className={\"dropdown-item\"} key={t.data.number} onClick={() => openTeam(t.data.number)}>{t.data.displayNumber}</button>)\n    return  <div className={\"justify-content-center\"}>\n        { isConnected ? null : <div className={\"alert alert-danger text-center h6\"}>{t`DISCONNECTED`}</div> }\n        <NoteDialog show={showNote} setShow={setShowNote} note={activeNote} setNote={setActiveNote} saveNote={saveNote} slackEnabled={slackEnabled}/>\n        <div className={\"ftaToolbar\"}>\n            {leftToolbar}\n            <div className={\"ftaToolbarRight\"} style={{marginLeft: \"auto\"}}>\n                <button className={\"btn btn-secondary toolbarBtn dropdown-toggle\"} data-toggle={\"dropdown\"} aria-haspopup={\"true\"} aria-expanded={\"false\"}>{t`Teams`}</button>\n                <div className={\"dropdown-menu\"} aria-labelledby={\"teamsBtn\"}>\n                    {teams}\n                </div>\n                <button className={\"btn btn-secondary toolbarBtn\"} onClick={() => setView('list')}><i className={\"fa fa-list\"}/></button>\n            </div>\n        </div>\n        {content}\n    </div>\n}","import FTANotepad from \"./FTANotepad\";\nimport LocalErrorBoundary from \"../LocalErrorBoundary\";\nimport { createRoot } from \"react-dom/client\";\n\nconst root = createRoot(document.getElementById(\"reactContainer\"));\nroot.render(<LocalErrorBoundary>\n        <a href=\"../\">&lt;&lt; {t`Back to Event Home`}</a>\n        <div className=\"row justify-content-center\">\