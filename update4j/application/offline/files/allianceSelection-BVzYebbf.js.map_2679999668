{"version":3,"file":"allianceSelection-BVzYebbf.js","sources":["../../../../../src/main/javascript/matchControl/api/allianceSelection.ts"],"sourcesContent":["import {SelectionMethod, SelectionOp} from \"ftc-common/javaTypes\";\n\nexport async function startSelection(eventCode: string, startTime: string) {\n    const res = await fetch(`/event/${eventCode}/control/selection/start/`, {\n        method: 'POST',\n        body: new URLSearchParams({ startTime })\n    })\n    if(!res.ok) {\n        return null;\n    }\n    return await res.json();\n}\n\nexport async function submitSelection(eventCode: string, team: number, type: SelectionOp, method: SelectionMethod) {\n    const res = await fetch(`/event/${eventCode}/control/selection/`, {\n        method: 'POST',\n        body: new URLSearchParams({ team: team.toString(), type, method })\n    })\n    if(!res.ok) {\n        return null;\n    }\n    return await res.json();\n}\n\nexport async function undoSelection(eventCode: string) {\n    const res = await fetch(`/event/${eventCode}/control/selection/`, {\n        method: 'DELETE'\n    })\n    if(!res.ok) {\n        return null;\n    }\n    return await res.json();\n}\n\nexport async function finishSelection(eventCode: string, startTime: string) {\n    const res = await fetch(`/event/${eventCode}/control/selection/finish/`, {\n        method: 'POST',\n        body: new URLSearchParams({ startTime })\n    })\n    return res.ok;\n}\n\nexport async function resetSelection(eventCode: string) {\n    const res = await fetch(`/event/${eventCode}/control/selection/reset/`, {\n        method: 'POST'\n    })\n    return res.ok;\n}\n\nexport async function addBackup(eventCode: string, seed: number, team: number) {\n    const res = await fetch(`/event/${eventCode}/control/selection/backup/`, {\n        method: 'POST',\n        body: new URLSearchParams({ seed: seed.toString(), team: team.toString() })\n    })\n    if(!res.ok) {\n        return null;\n    }\n    return await res.json();\n}\n\nexport async function removeBackup(eventCode: string, seed: number) {\n    const res = await fetch(`/event/${eventCode}/control/selection/backup/?seed=${seed}`, {\n        method: 'DELETE'\n    })\n    if(!res.ok) {\n        throw new Error(await res.text());\n    }\n    return await res.json();\n}"],"names":["startSelection","eventCode","startTime","res","submitSelection","team","type","method","undoSelection","finishSelection","resetSelection","addBackup","seed","removeBackup"],"mappings":"yUAEsB,eAAAA,EAAeC,EAAmBC,EAAmB,CACvE,MAAMC,EAAM,MAAM,MAAM,UAAUF,CAAS,4BAA6B,CACpE,OAAQ,OACR,KAAM,IAAI,gBAAgB,CAAE,UAAAC,CAAW,CAAA,CAAA,CAC1C,EACE,OAACC,EAAI,GAGD,MAAMA,EAAI,KAAK,EAFX,IAGf,CAEA,eAAsBC,EAAgBH,EAAmBI,EAAcC,EAAmBC,EAAyB,CAC/G,MAAMJ,EAAM,MAAM,MAAM,UAAUF,CAAS,sBAAuB,CAC9D,OAAQ,OACR,KAAM,IAAI,gBAAgB,CAAE,KAAMI,EAAK,SAAS,EAAG,KAAAC,EAAM,OAAAC,CAAQ,CAAA,CAAA,CACpE,EACE,OAACJ,EAAI,GAGD,MAAMA,EAAI,KAAK,EAFX,IAGf,CAEA,eAAsBK,EAAcP,EAAmB,CACnD,MAAME,EAAM,MAAM,MAAM,UAAUF,CAAS,sBAAuB,CAC9D,OAAQ,QAAA,CACX,EACE,OAACE,EAAI,GAGD,MAAMA,EAAI,KAAK,EAFX,IAGf,CAEsB,eAAAM,EAAgBR,EAAmBC,EAAmB,CAKxE,OAJY,MAAM,MAAM,UAAUD,CAAS,6BAA8B,CACrE,OAAQ,OACR,KAAM,IAAI,gBAAgB,CAAE,UAAAC,CAAW,CAAA,CAAA,CAC1C,GACU,EACf,CAEA,eAAsBQ,EAAeT,EAAmB,CAIpD,OAHY,MAAM,MAAM,UAAUA,CAAS,4BAA6B,CACpE,OAAQ,MAAA,CACX,GACU,EACf,CAEsB,eAAAU,EAAUV,EAAmBW,EAAcP,EAAc,CAC3E,MAAMF,EAAM,MAAM,MAAM,UAAUF,CAAS,6BAA8B,CACrE,OAAQ,OACR,KAAM,IAAI,gBAAgB,CAAE,KAAMW,EAAK,SAAS,EAAG,KAAMP,EAAK,UAAY,CAAA,CAAA,CAC7E,EACE,OAACF,EAAI,GAGD,MAAMA,EAAI,KAAK,EAFX,IAGf,CAEsB,eAAAU,EAAaZ,EAAmBW,EAAc,CAChE,MAAMT,EAAM,MAAM,MAAM,UAAUF,CAAS,mCAAmCW,CAAI,GAAI,CAClF,OAAQ,QAAA,CACX,EACE,GAAA,CAACT,EAAI,GACJ,MAAM,IAAI,MAAM,MAAMA,EAAI,MAAM,EAE7B,OAAA,MAAMA,EAAI,KAAK,CAC1B"}