            If you continue to see the message contact FIRST support.`}))}}async function lr(q,U){await $n(q,U)||Ee({label:ee`Failed to Lockout Match`,text:ee`Match lockout failed.  Please refresh and try again.`})}const ur=new Wt;function sr(q,U){let A;const d=new Promise(w=>{A=w}),s=document.createElement("div"),g=Bt.createRoot(s);async function S(w){g.unmount(),s.remove(),A(w)}return g.render(G.jsx(Zt,{client:ur,children:G.jsx(rr,{close:S,match:U,eventCode:q})})),d}function Jr(q,U,A){var C,I;const[d,s]=ae.useReducer(ir(U?y=>y.field===U:void 0),{activeType:Pe.QUALS,matches:[]}),[g,S]=ae.useReducer(or,{}),[w,m]=ae.useReducer(ar,{}),[v,c]=ae.useState({}),[r,e]=ae.useState(!1),[t,u]=ae.useState(!1),[a,l]=ae.useState(),[i,o]=ae.useState({}),[f,n]=ae.useState(!1),[h,k]=ae.useState({}),[_,E]=ae.useState([]),{isConnected:x,now:O}=Ut(`/stream/control/?code=${q}`,B),p=d.matches,b=p.filter(y=>y.isActive&&(y.state=="UNPLAYED"||y.state=="RANDOMIZED")).sort((y,P)=>P.loadedTime-y.loadedTime)[0],T=p.filter(y=>y.isActive&&y.state!="UNPLAYED"&&y.state!="RANDOMIZED").sort((y,P)=>P.start-y.start)[0];let N=-1;if(!b&&!(d.activeType=="PRACTICE"&&!t||d.activeType=="QUALS"&&!r)){let y=T?p.indexOf(T):0;y<0&&(y=0),N=((C=p.slice(y).find(P=>P.state==="UNPLAYED"))==null?void 0:C.number)||-1,N<0&&(N=((I=p.slice(0,y).find(P=>P.state==="UNPLAYED"))==null?void 0:I.number)||-1)}const{running:L,remaining:M}=Ht({startAt:T?T.start:null,duration:158,nowCallback:O});function B(y){mt(y)?(e(y.qualsActive),u(y.practiceActive),k(y.syncStatus),l(y.selectionState),o(y.timeoutData),E(y.displays),n(y.isArchived),c(y.settings),s(y),y.lastDisplayCommands.forEach(P=>S(P))):gt(y)?(A&&A(),s(y)):bt(y)?(s(y),y.showMeetRankings!==void 0&&c(P=>({...P,showMeetRankings:y.showMeetRankings})),y.isPhaseActive!==void 0&&y.matchType==Pe.QUALS&&e(y.isPhaseActive),y.isPhaseActive!==void 0&&y.matchType==Pe.PRACTICE&&u(y.isPhaseActive)):Ot(y)||_t(y)||Et(y)||wt(y)||Tt(y)||At(y)||St(y)||kt(y)||xt(y)||Nt(y)||Pt(y)||Rt(y)||qt(y)||Lt(