mallRobot = [t`Captain`, t`Partner`];\nconst labelsRobot = [t`Captain`, t`1<sup>st</sup> Pick`, t`2<sup>nd</sup> Pick`];\nconst labelsDean = [t`Congratulations`];\nconst labelsTopRanked = [1, 2, 3, 4, 5, 6].map(ranking => t`Rank ${ranking}`);\n\nexport function isRobotAward(award: Award) {\n    return [AwardId.WINNER_ID, AwardId.FINALIST_ID, AwardId.DIVISION_WINNER, AwardId.DIVISION_FINALIST, AwardId.CONFERENCE_FINALIST_ID].includes(award.awardTypeId);\n}\n\nexport function isDivisionAward(award: Award) {\n    return [AwardId.DIVISION_WINNER, AwardId.DIVISION_FINALIST].includes(award.awardTypeId);\n}\n\nexport function assignmentIsPopulated(assignment: AwardAssignment) {\n    return assignment.team || assignment.name;\n}\n\nexport function getAwardLabel(award: Award, series: number, count: number) {\n    let labels = labelsAward;\n    if (isRobotAward(award)) {\n        labels = count < 3 ? labelsSmallRobot : labelsRobot;\n    }\n    if (AwardId.DEANSLIST_ID == award.awardTypeId) {\n        labels = labelsDean;\n    }\n    if (AwardId.TOP_RANKED === award.awardTypeId) {\n        labels = labelsTopRanked;\n    }\n    const idx = series - 1;\n    return labels[idx >= labels.length ? labels.length - 1 : idx];\n}\n\nexport function sortAndFilterAssignments<AA extends AwardAssignment>(award: Award, assignments: AA[]) {\n    return assignments.filter(assignmentIsPopulated).toSorted((a, b) => isRobotAward(award) || AwardId.DEANSLIST_ID == award.awardTypeId ? (a.series - b.series) : (b.series - a.series));\n}\n\nexport type AdvancementEntry = CommonAdvancement & { index?: number };\nexport function filterAdvancements(advancements: AdvancementEntry[], count: number) {\n    const ret = []\n    let i = 0, foundAdvancements = 0;\n    for (; (i < advancements.length && foundAdvancements < count); i++) {\n        if (advancements[i]) {\n            ret.push(advancements[i])\n            if (advancements[i].status == AdvancementStatus.FIRST && advancements[i].team > -1) {\n                foundAdvancements++;\n            }\n        }\n    }\n    return ret\n}","import {\n    listAwardAssignments,\n    listAwards,\n    upsertScriptCustomizationContent,\n    upsertScriptCustomizationEnabled, useAwardPresentationContext\n} from \"./api\";\nimport {useMutation, useQuery, useQueryClient} from \"@tanstack/react-query\";\nimport {\n    AdvancementStatus,\n    Award,\n    AwardAssignment, AwardId,\n    AwardPresentationContext,\n    ScriptCustomization\n} from \"../javaTypes\";\nimport {\n    assignmentIsPopulated,\n    filterAdvancements,\n    getAwardLabel,\n    isDivisionAward,\n    sortAndFilterAssignments\n} from \"./utils\";\nimport './ViewScript.scss'\nimport {msgid, ngettext, t} from \"../i18n\";\nimport {ReactNode, useEffect, useId, useState} from \"react\";\nimport {Alert, Form} from \"react-bootstrap\";\n\nfunction StageDirection({children}: { children: ReactNode }) {\n    return <div className={\"text-muted ml-4\"}><i>{children}</i></div>\n}\n\nfunction AwardScript({award, assignments, divisions}: { award: Award, assignments: AwardAssignment[], divisions: Record<string, string> }) {\n    return <div className=\"award-item\">\n        <div className=\"mr-1 d-inline-block award-name\"><b>{award.name}</b></div>\n        <p className=\"award-desc\">\n            {award.script}\n            {isDivisionAward(award) ? <><br/>From the {divisions[award.awardSubtypeId]} division:</> : null}\n        </p>\n\n        <div className=\"ml-5 award-winners\">\n\n        {sortAndFilterAssignments(award, assignments).map(aa => <div className=\"award-winner-item\" key={aa.series}>\n            <div className=\"award-place\" dangerouslySetInnerHTML={{__html: getAwardLabel(award, aa.series, assignments.length)}}></div>\n            <div className=\"award-winner\">\n                {aa.comment ? <>\n                    {award.awardTypeId == AwardId.DEANSLIST_ID ? null : <div className=\"award-winner-comments-intro\">\n                        Here is what the