{"version":3,"file":"scripts-D4HijEe_.js","sources":["../../../../../../../../FTCCommon/src/main/javascript/awards/scripts/index.tsx"],"sourcesContent":["import {createRoot} from \"react-dom/client\";\nimport {QueryClient, QueryClientProvider} from \"@tanstack/react-query\";\nimport {ViewClosingScript, ViewOpeningScript} from \"../ViewScript\";\nimport {FTCConstants} from \"../../javaTypes\";\nimport {ComponentType} from \"react\";\n\ndeclare global {\n    interface Window {\n        eventCode: string;\n        eventName: string;\n        eventArchived: boolean;\n        ftcConstants: FTCConstants;\n    }\n}\n\n// Should never be hit, but more useful than a null fallback\nfunction NotFoundPage() {\n    return <div>\n        404 Not Found\n    </div>\n}\n\nconst container = document.getElementById('reactContainer');\nconst queryClient = new QueryClient();\nif(!container) {\n    console.error(\"Mount point for runtime container not found\")\n} else {\n    const root = createRoot(container);\n\n    let Component: ComponentType<{presentationOnly: boolean}> = NotFoundPage;\n    // Filter out local trailing slashes\n    const page = document.location.pathname.split('/').filter(x => x.length > 0).pop();\n    if(page === 'opening') {\n        Component = ViewOpeningScript;\n    } else if(page === 'closing') {\n        Component = ViewClosingScript;\n    } else {\n        throw new Error(`Unknown page: ${page}`);\n    }\n    root.render(<QueryClientProvider client={queryClient}>\n        {/* TODO a heading of some sort */}\n        <Component presentationOnly />\n    </QueryClientProvider>);\n}"],"names":["NotFoundPage","jsx","container","queryClient","QueryClient","root","createRoot","Component","page","x","ViewOpeningScript","ViewClosingScript","QueryClientProvider"],"mappings":"+zBAgBA,SAASA,GAAe,CACb,OAAAC,EAAA,IAAC,OAAI,SAEZ,eAAA,CAAA,CACJ,CAEA,MAAMC,EAAY,SAAS,eAAe,gBAAgB,EACpDC,EAAc,IAAIC,EACxB,GAAG,CAACF,EACA,QAAQ,MAAM,6CAA6C,MACxD,CACG,MAAAG,EAAOC,aAAWJ,CAAS,EAEjC,IA