{"version":3,"file":"StatusProgress-DfgH_RL1.js","sources":["../../../../../../FTCCommon/src/main/javascript/inspection/status/StatusProgress.tsx"],"sourcesContent":["import {InspectionUpdateStatus, Status} from \"../../javaTypes\";\n\nfunction getBar(status: InspectionUpdateStatus, statusData: Status[]) {\n    let count = statusData.filter(x => x === status.name).length;\n    const perc = 100 * count / statusData.length;\n    const title = `${Math.round(perc)}% ${status.displayName}`;\n    let text;\n    if (perc < 5) {\n        text = ''\n    } else if (perc < 25) {\n        text = `${Math.round(perc)}%`;\n    } else {\n        text = title;\n    }\n    return <div className=\"progress-bar\" hidden={count === 0} title={title} style={{width: `${perc}%`, backgroundColor: status.color, color: status.textWhite ? 'white' : 'black'}}>{text}</div>\n}\n/**\n * Returns a progress bar for each status\n * @param statusList The list of possible statuses\n * @param statusData The list of all team form statuses\n */\nexport default function StatusProgress({statusList, statusData} : {\n    statusList: InspectionUpdateStatus[],\n    statusData: Status[]\n}) {\n    if (!statusList || !statusList.length) return null;\n    // Force NONE to end & PASS to beginning\n    const reorderedStatuses = statusList.map((x,i) => Object.assign({}, {i: x.name === 'NONE' ? 99 : x.name === 'PASS' ? 0 : i}, x));\n    reorderedStatuses.sort((a,b) => a.i - b.i);\n    return <div className=\"progress\">\n        {reorderedStatuses.map(status => getBar(status, statusData))}\n    </div>\n}"],"names":["getBar","status","statusData","count","x","perc","title","text","jsx","StatusProgress","statusList","reorderedStatuses","i","a","b"],"mappings":"kXAEA,SAASA,EAAOC,EAAgCC,EAAsB,CAClE,IAAIC,EAAQD,EAAW,UAAYE,IAAMH,EAAO,IAAI,EAAE,OAChD,MAAAI,EAAO,IAAMF,EAAQD,EAAW,OAChCI,EAAQ,GAAG,KAAK,MAAMD,CAAI,CAAC,KAAKJ,EAAO,WAAW,GACpD,IAAAM,EACJ,OAAIF,EAAO,EACAE,EAAA,GACAF,EAAO,GACdE,EAAO,GAAG,KAAK,MAAMF,CAAI,CAAC,IAEnBE,EAAAD,EAEJE,MAAC,OAAI,UAAU,eAAe,OAAQL,IAAU,EAAG,MAAAG,EAAc,MAAO,CAAC,MAAO,GAAGD,CAAI,IAAK,gBAAiBJ,EAAO,MAAO,MAAOA,EAAO,UAAY,QAAU,OAAO,EAAI,SAAKM,CAAA,CAAA,CAC1L,CAMA,SAAwBE,EAAe,CAAC,WAAAC,EAAY,WAAAR,GAGjD,CACC,GAAI,CAACQ,GAAc,CAACA,EAAW,OAAe,OAAA,KAExC,MAAAC,EAAoBD,EAAW,IAAI,CAACN,EAAEQ,IAAM,OAAO,OAAO,CAAI,EAAA,CAAC,EAAGR,EAAE,OAAS,OAAS,GAAKA,EAAE,OAAS,OAAS,EAAIQ,GAAIR,CAAC,CAAC,EAC/H,OAAAO,EAAkB,KAAK,CAACE,EAAEC,IAAMD,EAAE,EAAIC,EAAE,CAAC,EAClCN,EAAA,IAAC,MAAI,CAAA,UAAU,WACjB,SAAAG,EAAkB,IAAIV,GAAUD,EAAOC,EAAQC,CAAU,CAAC,CAC/D,CAAA,CACJ"}